		{
			$where .= "((tr.in_status = '1') || (tr.in_status = '2' && tr.dt_target >= current_date()))";
		}
		else if($status == '2')
		{
			$where .= "(tr.in_status = '2' && tr.dt_target < current_date())";
		}
		else 
