function create_leg(id)
{
	$.ajax({
			type: "POST",
			url: base_url+'org_transportation/get_airline_covering_leg_details',
			data: {
				in_id:id
			}, 
			async:false,	
			dataType:"json",
			success:function(data) 
			{
				$("#add_aviation_leg").trigger( "click" );
				$('#cover_aviation_leg').val(id);
				$('#pickup_airport1').val(pick_up);
				$('#pickup_airport_identifer').val(pickup_airport);
				$('#dropoff_airport1').val(drop_off);
				$('#dropoff_airport_identifer').val(dropoff_airport);
				$("#pickup_airport_clock1").val(data['start_time']);
				$("#dropoff_airport_clock1").val(data['end_time']);
				$('#assign_user_name-pilot1').val(data['st_display_name']);
				$('#pickup_airport_zone1').val(data['time_zone']);
				$('#dropoff_airport_zone1').val(data['time_zone']);
				$('#assignuid-pilot1').val(data['in_user_id']);
				$("#pickup_airport_clock1_year").val(data['start_time_year']);
				$("#dropoff_airport_clock1_year").val(data['end_time_year']);
			}
		});

}

