.dashboard #middle.orgtabs {border-top: 53px solid #6c6d71; padding:18px 0;}
.dashboard #middle.orgtabs .tabular {margin-top:-64px;}
.dashboard #middle.orgtabs .nav-tabs {margin: 0 0 -4px; border: 0px;}
.dashboard #middle.orgtabs .nav-tabs>li a, .dashboard #middle.orgtabs .nav-tabs>li.active a {background-color: transparent !important; padding: 0px; border:0;}
.dashboard #middle.orgtabs .nav-tabs>li a:hover, .dashboard #middle.orgtabs .nav-tabs>li.active a:hover {background-color: transparent !important; }
.dashboard #middle.orgtabs .nav-tabs>li a:hover span, .dashboard #middle.orgtabs .nav-tabs>li.active a span {color: #f35613 !important; background-color: #fff;}
.dashboard #middle.orgtabs .nav-tabs>li.active:first-child a:before {background-color: #fff; background-image: none;}
.dashboard #middle.orgtabs .nav-tabs>li:first-child a:hover:before {background-color: #fff; background-image: none;}
.dashboard #middle.orgtabs .nav-tabs>li a span {color: #6c6d71; text-decoration: none; display: inline-block; font: 500 18px/24px 'Roboto',sans-serif !important; text-align: center; background-color: #f2f0f0; vertical-align: top; border-top: 1px solid #56565a;
    border-width: 1px 0 0 !important; margin: 1px 0 -1px; padding: 10px 30px; }
.ie11 .dashboard #middle.orgtabs .nav-tabs>li.first a span {margin-left:-1px;}
.dashboard #middle.orgtabs .nav-tabs>li:first-child a:before {content: ""; background-image:none; width: 10px; background-color: #f2f0f0;  border-top-left-radius: 6px; margin-left: 0; border: 1px solid #56565a; border-width: 1px 0 0 1px; height: 44px;}
.dashboard #middle.orgtabs .nav-tabs>li a:before {content: ""; background: url(../images/tabs-image.png) no-repeat left top; width: 33px; height: 46px; display: inline-block; margin-left: -25px; z-index: 1; position: inherit;}
.dashboard #middle.orgtabs .nav-tabs>li a:after {content: ""; background: url(../images/tabs-image.png) no-repeat right top; width: 33px; height: 46px;display: inline-block; z-index: 2; position: inherit;}
.dashboard #middle.orgtabs .nav-tabs>li a:hover:before {content: ""; background: url(../images/tabs-image.png) no-repeat left bottom; z-index: 3;}
.dashboard #middle.orgtabs .nav-tabs>li a:hover:after {content: ""; background: url(../images/tabs-image.png) no-repeat right bottom; z-index: 3;}
.dashboard #middle.orgtabs .nav-tabs>li.active a:before {content: ""; background: url(../images/tabs-image.png) no-repeat left bottom; z-index: 2;}
.dashboard #middle.orgtabs .nav-tabs>li.active a:after {content: ""; background: url(../images/tabs-image.png) no-repeat right bottom;}
.dashboard #middle.orgtabs .collapse {display: none;}
.dashboard #middle.orgtabs .panel-heading {display: none;}
.dashboard #middle.orgtabs .panel-default {border:none; border-radius:0!important; box-shadow:none!important;}
.dashboard #middle.orgtabs .panel-body {padding:43px 0 0;}


@media(min-width:768px){    
.dashboard #middle.orgtabs .panel-heading {display:none;}  
.dashboard #middle.orgtabs .panel-collapse {height:auto;}
.dashboard #middle.orgtabs .panel-collapse.collapse {display:block;}
}

@media (max-width:1200px) {
.dashboard #middle.orgtabs .nav-tabs>li:first-child a:before {height:45px;}
}

@media (max-width:767px) {
.dashboard #middle.orgtabs {border:none; padding-top:72px;}
.dashboard #middle.orgtabs .nav-tabs {display: none;}
.dashboard #middle.orgtabs .tab-content .tab-pane {display: block;}
.dashboard #middle.orgtabs .panel-heading {display: block;}
/*.dashboard #middle.orgtabs .panel-default {margin-bottom:10px; border: 1px solid #ddd; border-radius: 0px !important;}*/
.dashboard #middle.orgtabs .panel-default {margin-bottom:10px; border:none; border-radius:0!important;}
.dashboard #middle.orgtabs .panel-heading {padding: 0; border-bottom:1px solid transparent; -webkit-border-radius:6px; -moz-border-radius:6px; -ms-border-radius:6px; -o-border-radius:6px; border-radius:6px; background:#fff; border:1px solid #F05928;}
.dashboard #middle.orgtabs .panel-title a {display: block;}
.dashboard #middle.orgtabs .collapse.in {display: block;}
.dashboard #middle.orgtabs .dashboard-page .main-content-wrapper {border-top-width: 10px;}
.dashboard #middle.orgtabs .org-dashboard-transport {margin-top: -10px; padding-right: 0;}
.dashboard #middle.orgtabs .tab-content {margin-bottom: 20px;}
.dashboard #middle.orgtabs .panel-title a {text-decoration:none; text-transform:uppercase; text-align:center; font-size:25px; color:#F05928; background:url(../images/minus-account.png) no-repeat 97% 10px; line-height:23px; padding:14px 15px 16px;}
.dashboard #middle.orgtabs .panel-title a.collapsed {background:url(../images/plus-account.png) no-repeat 97% 10px;}
}

@media (max-width:667px) {
.dashboard #middle.orgtabs .panel-title a {font-size:19px; background:url(../images/minus-account-mobile.png) no-repeat 97% 10px; line-height:20px; padding:9px 15px 11px;}
.dashboard #middle.orgtabs .panel-title a.collapsed {background:url(../images/plus-account-mobile.png) no-repeat 97% 10px;}
}