$(document).on('click','#submit-recommend',function(e){
	
	var origin_add = $("#origin-address1").val();
	var destination_add = $("#destination-address1").val();
	
	var source_details = [];
	var dest_details = [];
	var start_add = $('.origin-address-1').length;
	var end_add = $('.destination-address-1').length;
	var error_msg = '';
	var error_reporting = 0;
	var emailReg = /^([\w-\.\+]+@([\w-]+\.)+[\w-]{2,4})?$/;
					
	if(start_add > 0 && end_add > 0 )
	{
		for(var i=0; i<start_add; i++)
		{
			var coun = parseInt(i)+parseInt(1);	
			source_details.push({"pickup_city":addslashes($("#origin-city-"+coun).val()),"pickup_state":addslashes($("#origin-state-"+coun).val()),"pickup_country":addslashes($("#origin-country-"+coun).val()),"pickup_zip":addslashes($("#origin-zip-"+coun).val()),"dropoff_city":addslashes($("#dest-city-"+coun).val()),"dropoff_state":$("#dest-state-"+coun).val(),"dropoff_country":$("#dest-country-"+coun).val(),"dropoff_zip":addslashes($("#dest-zip-"+coun).val())});
		}						
	}	
	
	var origin_comments = $('#origin-Comments').val();	
	var city = $("#locality_s").val();
	var state_abbr = $("#administrative_area_level_1_s").val();
	var zip_code = $("#postal_code_s").val();
	var country = $("#country_s").val();
	var city_dest = $("#locality_d").val();
	var state_abbr_dest = $("#administrative_area_level_1_d").val();
	var zip_code_dest = $("#postal_code_d").val();
	var country_dest = $("#country_d").val();
	var start_array = {"city":city,"state_abbr":state_abbr,"zip_code":zip_code,"country":country};
	var end_array = {"city":city_dest,"state_abbr":state_abbr_dest,"zip_code":zip_code_dest,"country":country_dest};

	originAddress = start_array;		
	destinationAddress = end_array;
	
	if(!originAddress && !zip_code)
	{	
		$.ajax({
				type: 'POST',
				url: base_url+'org_transportation/get_state_city_country_by_zipcode/',
				dataType: 'json',
				async: false,
				cache: false,
				data: { 'zip_code' : origin_add.trim()},
				success : function(result){
				if(!$.trim(result))
				{
									//alert('result get_state_city_country_by_zipcode pickup type'+result);
									alert("Pick Up Address is invalid");
									$("#origin-address1").val(""); 
									origin_add = '';
									return false;
				}
				else
				{
									//console.log(result.city);
									var zip_code_origin = origin_add.trim();
									var full_address = result.city;
									var city = result.city;
									var state_abbr = result.state_prefix;
									var country = result.st_country;
									var start_array = {"city":city,"full_address":full_address,"state_abbr":state_abbr,"zip_code":zip_code_origin,"country":country};
									//console.log(start_array);									
									originAddress = 	start_array;
									
				}
			}
		});
	}
	
	var zip_code_d = $("#postal_code_d").val();
	if(!destinationAddress && !zip_code_d)
	{
		$.ajax({
				type: 'POST',
				url: base_url+'org_transportation/get_state_city_country_by_zipcode/',
				dataType: 'json',
				async: false,
				cache: false,
				data: { 'zip_code' : destination_add.trim()},
				success : function(result)
				{
					if(!$.trim(result))
					{
						alert("Drop Off Address is invalid");
						$("#destination-address1").val("");
						destination_add = '';
						return false;
					}else
					{
						var zip_code_dest = destination_add.trim();
						var full_address = result.city;
						var city = result.city;
						var state_abbr = result.state_prefix;
						var country = result.st_country;
						var end_array = {"city":city,"full_address":full_address,"state_abbr":state_abbr,"zip_code":zip_code_dest,"country":country};									
						destinationAddress = 	end_array;
					}
				}
			});
	}
	if(originAddress.city =='' || originAddress.state_abbr == '')
	{
		error_msg += "Pick Off Address is invalid \n";
		error_reporting = 1;
		
	}

	if(destinationAddress.city =='' || destinationAddress.state_abbr == '')
	{
		error_msg += "Drop Off Address is invalid \n";
		error_reporting = 1;
	}

	if (origin_add =='' || destination_add =='' )
	{
		error_msg += "Please, select two different points on route! \n";
		error_reporting = 1;
	}
	
	if($('#contact_name').val()=='')
	{
		error_reporting = 1;	
		error_msg += 'Please Enter User Name \n';
	}
					
	if($('#contact_email').val()=='')
	{
		error_reporting = 1;	
		error_msg += 'Please Enter User Email Address \n';
	} else if(!emailReg.test( $('#contact_email').val() ))
	{
		error_reporting = 1;
		error_msg += 'Please enter User Valid Email Address \n';
	}

	if(error_reporting ==1)
	{
		alert(error_msg);
		return false 
	} else 
	{	
					s = base_url+'org_transportation/send_routingtransport_recommend_driver/';
					i = {
							origin_address: originAddress,
							destination_address: destinationAddress,
							//telephone:telephone,
							routing_details:source_details,
							origin_comments:origin_comments,
							user_name :$('#contact_name').val(),
							user_email : $('#contact_email').val()
						},	
					jQuery('#leg_reorder_loader').modal('show'),
					jQuery("#can-save").html(''),
					$.post(s, i).done(function (data) { //console.log(data);
							jQuery('#leg_reorder_loader').modal('hide');
							window.location.href = base_url+'user-signup';
							//window.location.reload();
							//return window.location.href=host2+window.location.pathname;//window.location.reload()
					});
              
	
	}
	});
	
