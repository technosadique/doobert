@font-face {
    font-family: 'SofiaProbold';
    src: url('SofiaProSoftW01-Bold.eot');
    src: url('SofiaProSoftW01-Bold.eot?#iefix') format('embedded-opentype'),
        url('SofiaProSoftW01-Bold.woff2') format('woff2'),
        url('SofiaProSoftW01-Bold.woff') format('woff'),
        url('SofiaProSoftW01-Bold.ttf') format('truetype'),
        url('SofiaProSoftW01-Bold.svg#SofiaProSoftW01-Bold') format('svg');
    font-weight: bold;
    font-style: normal;
}

