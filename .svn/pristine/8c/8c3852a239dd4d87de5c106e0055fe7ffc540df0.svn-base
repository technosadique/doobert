	{
			
			var idd = $(this).attr("id");
			if(idd != 'is_round'){
					if ($(this).is(':checked')) 
					{
						$(this).next('.slider').html('<span class="yes">Yes</span>');
						var leg_number = $(this).attr('data-id');
						
						var leg_id = $(this).val();
						console.log(leg_number);								
							if(confirm('Are you sure, you want to mark this leg completed?'))
							{         
								$("#leg_completed_id").val(leg_id);
								$('#leg_completed_no').val(leg_number);
								document.mark_complete.submit();
							}
							else
							{
								$(this).prop("checked",false);
								$(this).next('.slider').html('<span class="no">No</span>');	
							}
						
					}
					else
					{
						$(this).next('.slider').html('<span class="no">No</span>');
					}
			}	
			
	});
});
