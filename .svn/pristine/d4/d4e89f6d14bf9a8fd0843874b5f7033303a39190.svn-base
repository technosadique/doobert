var temp_amount = [];

$(document).on('click', '.remove_current', function(){
	$(this).closest('.form-group').remove();
	
	temp_amount = [];
	var sum = 0;
	
	$(".cost-amt").each(function(val) {
		var textvalue = $(this).closest('.form-group').find('.cost_desc_chk').val();
		
		if($.trim(textvalue) != '') {
			temp_amount.push($(this).val());
		}
	});
	
	for(var i=0;i<temp_amount.length;i++) {
		sum = Number(sum)+Number(temp_amount[i]);
	}
	//console.log(sum);
	$('.total').text('$'+sum.toFixed(2));
});

//$('.cost-amt').blur(function() {
$(document).on('blur', '.cost-amt', function(){
	temp_amount = [];
	var sum = 0;
	
	$(".cost-amt").each(function(val) {
		if($.isNumeric($(this).val()) == true) {
			var textvalue = $(this).closest('.form-group').find('.cost_desc_chk').val();
			if($.trim(textvalue) != '') {
				temp_amount.push($(this).val());
			}
		} else {
			alert('Please enter numeric values');
			return false;
		}
	});
	
	
	for(var i=0;i<temp_amount.length;i++) {
		sum = Number(sum)+Number(temp_amount[i]);
	}
	
	$('.total').text('$'+sum.toFixed(2));
});

$(document).on('click', '.trash_default', function(){

	if($.trim($('#cost-description1').val())!='' && $.trim($('#cost1').val()) != '') {
		$(this).closest('.form-group').remove();
		
		temp_amount = [];
		var sum = 0;
		
		$(".cost-amt").each(function(val) {
			temp_amount.push($(this).val());
		});
		
		for(var i=0;i<temp_amount.length;i++) {
			sum = Number(sum)+Number(temp_amount[i]);
		}
		
		$('.total').text('$'+sum.toFixed(2));
	}
});
