function get_alert(id)
{	
	var ids="#checkboxtacceptDog_"+id;
	var priv ="#animal_privacy_"+id;
	var result1 = false; 
	$('#selectedid').val(id);	
	if($(ids).is(":checked"))
	{
		if($(priv).val()==1){	
		$('#animal_id').val(id);	
		$('#animal-transfer-continue').modal('show');
		return result1;	
		}
		else {
			$.ajax({
			  type: "POST",
			  url: '<?php echo base_url(); ?>org_animal/set_transfer_yes',
			  beforeSend: function(){
				  $('#loader').show();
			  },
			  data: {
				  animal_id:id
			  },
			  dataType:"json",
			  
			  complete: function(){
				 $('#loader').hide();
			  },
			  success:function(data){
				if(data.result==1)
				{
						$(ids).attr('checked');
						$(ids).val(1);
						//alert(priv);
						$(priv).val(0);
					 // location.reload();
				}
			  }	
			});
		}
	}else {
			$.ajax({
			  type: "POST",
			  url: '<?php echo base_url(); ?>org_animal/set_transfer_No',
			  beforeSend: function(){
				  $('#loader').show();
			  },
			  data: {
				  animal_id:id
			  },
			  dataType:"json",
			  
			  complete: function(){
				 $('#loader').hide();
			  },
			  success:function(data){
				if(data.result==1)
				{
					$(ids).removeAttr('checked');
					$(ids).val(0);
					// location.reload();
				}
			  }	
			});
		
	result1 = true;
	return result1;	
	}
}
