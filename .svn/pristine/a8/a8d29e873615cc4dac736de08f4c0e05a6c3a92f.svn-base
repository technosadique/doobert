function update_password(){
  var current_url = window.location.pathname;  
  var str_array = current_url.split('/');
  var org_id = str_array[str_array.length-1];  
  var stPass = $('#st_password').val();
  if(stPass){
    $.ajax({
        type: "POST",
        url: base_url+'administration.php/arpa_organization/update_password',
        beforeSend: function(){
        $('#loader').show();
        },
        data: {
        org_password:stPass,
        org_id:org_id
        },
        dataType:"json",
      
        complete: function(){
        
        },
        success:function(data){
        $('#loader').hide();
          
        if(data.res=='success')
        {
          alert('Password reset successfully.');
          location.reload();
        }else if(data.res =='failure'){
          alert('No Change.');
          window.location = current_url;  
        }
      }
    });
  }else{
    alert("Password field should not be blank.");
    return false;
  }

  $('#org-password').modal('hide');
}

</script>

