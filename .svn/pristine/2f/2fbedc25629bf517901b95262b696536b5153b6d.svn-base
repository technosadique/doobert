function checkImageSrc(img) {
	var image = new Image(); 
	
	image.src = img;
	
	if (image.width == 0) {
		return false
	} else {
		return true
	}
}

function addslashes(str) {
	if(str)
	{
		str=str.replace(/\\/g,'\\\\');
		str=str.replace(/\'/g,'\\\'');
		str=str.replace(/\"/g,'\\"');
		str=str.replace(/\0/g,'\\0');
		return str;
	}
}
function stripslashes(str) {
	if(str)
	{
		str=str.replace(/\\'/g,'\'');
		str=str.replace(/\\"/g,'"');
		str=str.replace(/\\0/g,'\0');
		str=str.replace(/\\\\/g,'\\');
		return str;
	}
}

