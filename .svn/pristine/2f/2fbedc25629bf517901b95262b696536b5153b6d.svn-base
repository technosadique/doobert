function removePlaneDriver(did,vehicle_id)
{ 		
	 if(confirm('Are you sure you want to delete this record?'))
	 {	 
	 	//alert('did'+did);
		//alert('vehicle_id'+vehicle_id);
		
		$('#vehiclediv_'+did).hide(100);
