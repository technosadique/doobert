});
  
  $('.switch-container input[type="checkbox"]').each(function(){
		if($(this).is(':checked')){
			$(this).next('.slider').append('<span class="yes">Yes</span>');
		}else{
			$(this).next('.slider').append('<span class="no">No</span>');
		}
	})
	
	$('.switch-container input[type="checkbox"]').on('click', function () {
		$(this).next('.slider').empty();
