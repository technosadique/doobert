<?php $base_url = $this->config->item('base_url'); 
$edit_page_url = '';
$incomplete_transport_request = '0';
if(isset($transportation_list['transportation_list']) && count($transportation_list['transportation_list'])>0) 
{
	
	$transportation_details = $transportation_list['transportation_list']['0'];	
	$transportation_ids = $transportation_details['transportation_id'];
	if($transportation_details['transport_status']=='Airline') 
	{ 
			$edit_page_url = base_url().'edit-airline-transport?id='.$transportation_details['transportation_id'];
			$incomplete_transport_request= '1';	
	} else if($transportation_details['transport_status']=='Ride') 
	{ 
			$edit_page_url = base_url().'edit-new-ride?id='.$transportation_details['transportation_id'];
			$incomplete_transport_request= '1';	
	} else if($transportation_details['transport_status']=='Route') 
	{ 
			$edit_page_url = base_url().'edit-route?id='.$transportation_details['transportation_id'];
			$incomplete_transport_request= '1';	
	}else
	{ 
			$incomplete_transport_request= '1';	
			$edit_page_url = base_url().'org-edit-transport?id='.$transportation_details['transportation_id'];
	}
}

