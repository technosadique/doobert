body.green {background:#fff url(../../img/green/pagebg.gif) repeat-x !important;}
body.green img {max-width:100%;}
body.green #wrap {max-width:997px; margin:0 auto; background:#fff;}
.page-header-fixed .page-container.mart0 {margin-top:0;}


body.green .progress > .progress-bar-info{background-color: #3fe233;}
body.green .donation-form{border: 1px solid #d0e8ce;}
body.green .donation-form h2{ color:#097001;}
body.green .donation-form .form-content .control-label.special{color:#097001;}
body.green .donation-form .form-content .controls.amount .radio{ color:#097001;}
body.green .donation-form .form-content .controls input{background: #f2f2f2; /* Old browsers */
/* IE9 SVG, needs conditional override of 'filter' to 'none' */
background: url(data:image/svg+xml;base64,) !important;
background: -moz-linear-gradient(top,  #f2f2f2 0%, #ffffff 14%, #ffffff 100%) !important; /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f2f2f2), color-stop(14%,#ffffff), color-stop(100%,#ffffff)) !important; /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  #f2f2f2 0%,#ffffff 14%,#ffffff 100%) !important; /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  #f2f2f2 0%,#ffffff 14%,#ffffff 100%) !important; /* Opera 11.10+ */
background: -ms-linear-gradient(top,  #f2f2f2 0%,#ffffff 14%,#ffffff 100%) !important; /* IE10+ */
background: linear-gradient(to bottom,  #f2f2f2 0%,#ffffff 14%,#ffffff 100%) !important; /* W3C */
color:#787878 !important; font:normal 18px/22px 'Roboto',sans-serif; padding: 10px 9px !important; height:auto; border:1px solid #d2d2d2;
}
/* donation form step 1 - rightpart*/
body.orange .recent-donations h2{ border-bottom:1px solid #d9f4d7;}
body.green .recent-donations .bg {background:#e5ffe3; }
body.green .recent-donations .donation-comment{border:1px solid #d0e8ce;}
body.green .recent-donations .donation-comment p.amount{ color:#097001; }
body.green .donation-rightpart .rightpart-box .bg .view-profile, body.green .donation-rightpart .rightpart-box .bg .view-profile:hover, body.green .donation-rightpart .rightpart-box .bg p span{color:#097001;}/*0A558E*/


/*Profile Title*/
body.green .donation-detail .page-title { background:#1679C4 url(../../img/green/pagebg.gif) repeat-x 0 -145px;}

/*progress & donation/pledge detail*/
body.green .progress {box-shadow:0 1px 2px rgba(0, 0, 0, 0.1) inset!important; background-image:linear-gradient(to bottom, #E6E6E6, #ECECEC)!important;}
body.green .progress .bar {background-color:#6abfff; background-size: 70px 70px;}
body.green .raised-amount, body.green .goal-amount {background:url(../../img/white-transperent-bg.png) repeat 0 0; color:#075c01; }

body.green .btnDonatePledge {color:#fff; border-bottom:2px solid #096501; background: #33be28; /* Old browsers */
/* IE9 SVG, needs conditional override of 'filter' to 'none' */
background: url(data:image/svg+xml;base64,);
background: -moz-linear-gradient(top,  #33be28 0%, #0b8e01 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#33be28), color-stop(100%,#0b8e01)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  #33be28 0%,#0b8e01 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  #33be28 0%,#0b8e01 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top,  #33be28 0%,#0b8e01 100%); /* IE10+ */
background: linear-gradient(to bottom,  #33be28 0%,#0b8e01 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#33be28', endColorstr='#0b8e01',GradientType=0 ); /* IE6-8 */}  

body.green .btnDonatePledge:hover, body.green .btnDonatePledge:focus{color:#fff; background: #33be28; /* Old browsers */
/* IE9 SVG, needs conditional override of 'filter' to 'none' */
background: url(data:image/svg+xml;base64,);
background: -moz-linear-gradient(top,  #0b8e01 0%, #33be28 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#0b8e01), color-stop(100%,#33be28)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  #0b8e01 0%,#33be28 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  #0b8e01 0%,#33be28 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top,  #0b8e01 0%,#33be28 100%); /* IE10+ */
background: linear-gradient(to bottom,  #0b8e01 0%,#33be28 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#0b8e01', endColorstr='#33be28',GradientType=0 ); /* IE6-8 */}

body.green .time {background:#eef5ee url(../../img/icon-clock.png) no-repeat 9px center; border:3px solid #e6f0e5; color:#2a2a2a;}
body.green .time .day-remaining {color:#2a2a2a;}
body.green .time .text {color:#2a2a2a; padding-left: 8px;}

body.green .donation-needed .donation-slider { background:url(../../img/green/slider.png) repeat-x 0px 8px;}
body.green .donation-needed .donation-slider .slider {background:url(../../img/green/slider.png) repeat-x 0px -38px; }
body.green .donation-needed .donation-slider .slider span {border:1px solid #585858; color:#fff; background: #0fa703; /* Old browsers */
/* IE9 SVG, needs conditional override of 'filter' to 'none' */
background: url(data:image/svg+xml;base64,);
background: -moz-linear-gradient(top,  #0fa703 0%, #097a01 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#0fa703), color-stop(100%,#097a01)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  #0fa703 0%,#097a01 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  #0fa703 0%,#097a01 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top,  #0fa703 0%,#097a01 100%); /* IE10+ */
background: linear-gradient(to bottom,  #0fa703 0%,#097a01 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#0fa703', endColorstr='#097a01',GradientType=0 ); /* IE6-8 */}

body.green .donation-needed .total {background:#f5f5f5; color:#0daa01;}

body.green .social-icons {margin:18px 0 10px;}
body.green .social-icons img {margin:0 5px 10px 0;}

body.green .donation-pledge-detail .article-block .main-article {border:1px solid #b3e5af;}
body.green .donation-pledge-detail .article-block .main-article .bg {background:#f8f8f8; }
body.green .donation-pledge-detail .article-block .main-article .bg p span {color:#097001;}
body.green .donation-pledge-detail .article-block .main-article input.fb-link { border:1px solid #e2e2e2; background:#fff; color:#4b7276;}
body.green .donation-pledge-detail .article-block .main-article input.fb-link:focus{border-color: #999999;}
body.green .donation-pledge-detail .article-block .main-article .btnShare {background:url(../../img/btn-share.gif) no-repeat 0 0;}
body.green .view-profile {color:#097001; background: url(../../img/green/arrow-right.gif) no-repeat right 8px; }
body.green .view-profile:hover {color:#a11d1d; }
body.green .donation-pledge-detail h1 {color:#097001;}
body.green .donation-pledge-detail .media .posted-by{ color:#8d8c8c;}
body.green .donation-pledge-detail .media .posted-by span{color:#097001;}
body.green .donation-pledge-detail .media .media-comment{ border:1px solid #e8e8e8;}
body.green .donation-pledge-detail .media .media-heading{color:#097001; background:url(../../img/comment-bubble-top.gif) no-repeat 0px 0px; }

