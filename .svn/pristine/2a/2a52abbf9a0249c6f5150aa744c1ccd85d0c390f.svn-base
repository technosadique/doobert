		$form_field				=	array('animals_list' => "");
		if($this->input->post('action')=='continue')
		{
			$transport_id = $this->input->post('current_transport_id');
			
			$this->orgtrasnportation_model->update_current_trasport_animal();
			$animal_id = array();
