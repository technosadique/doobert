    }
	
	public function send_animal_update_notification($transport_id = '', $cood_id='', $org_id='', $tid= '', $target_dt='', $pickupdate='',$code='',$orglogopath='') 
	{   
		$this->db->select('tp.in_user_id,u.strDeviceId,u.strDeviceToken');
		$this->db->from('tbl_transportation_participants tp');
		$this->db->join('tbl_users u',"u.in_user_id = tp.in_user_id and u.in_deleted='0' and u.strDeviceId!='' and u.strDeviceToken!='' and u.in_pushnotification='0'",'INNER');
		$this->db->join('tbl_push_notification pn',"pn.in_user_id = u.in_user_id",'LEFT');		
		$where='';
		$where .= "(tp.in_transportation_id='".$transport_id."')";
		$where .= "AND (pn.in_flg_trans_ani IS NULL OR pn.in_flg_trans_ani = 1)";	
		$this->db->where($where);			
		//$this->db->where('tp.in_transportation_id',$transport_id);
		//$this->db->where('pn.in_flg_trans_ani','1');
		$this->db->group_by('tp.in_user_id');		
		$query = $this->db->get();			
		$user_details = $query->result_array();	
		
		
		if(isset($user_details) && !empty($user_details))
		{
			foreach ($user_details as $val) 
			{	
				
				$raw_message = 'Transport #'.$code.$transport_id. ' animal list updated'; // pass message
				if(isset($raw_message) && $raw_message != '')
				{	
					$deviceToken = $val['strDeviceToken']; 	
					
					/*if($deviceToken=='f0561850f779acc14c2026ed10baf49ac868e2c1a0a727990923c317bf35ff63' 
					|| $deviceToken=='8507e665c045bc46a3436e9e90dd0137cbd5e13fa20f6822c450834fa25ebb5d' 
					|| $deviceToken=='ef20b49877be9b311b5dae43f86623e34c187adba040c1dec5b35c51390d89b5'
					|| $deviceToken=='0cbe7540b393a12431fd32268b67009705d429b22d8192a9f247d312e52f8102'){*/		
					
					$passphrase = 'DRftgyhu12';				
					
					$message = $raw_message;
			
					$ctx = stream_context_create();				
					stream_context_set_option($ctx, 'ssl', 'local_cert', $_SERVER['DOCUMENT_ROOT'].'/upload/cert/doobert_adhoc.pem');
					stream_context_set_option($ctx, 'ssl', 'passphrase', $passphrase);
					
					$fp = stream_socket_client(
						'ssl://gateway.push.apple.com:2195', $err,
						$errstr, 60, STREAM_CLIENT_CONNECT|STREAM_CLIENT_PERSISTENT, $ctx);
					
					
					if (!$fp)
						exit("Failed to connect: $err $errstr" . PHP_EOL);
					
					echo $deviceToken .'<br />';
					echo 'Connected to APNS' . PHP_EOL;
					
					
					$body['aps'] = array(
						'alert' => $message,
						'sound' => 'default',
						'category' => '',
						'mutable-content' => '1'
						);
					$body['Info'] = array(
						'message_type' 		   => 'feed',
						'in_transportation_id' => $transport_id,
						'participant_id'	   => array($val['in_user_id']),
						'cood_user_id'		   => $cood_id,
						'organization_id'	   => $org_id,
						'dt_pickup'            => $pickupdate,
						'dt_target' 		   => $target_dt,
						'org_code'             => $code,
						'orglogo'			   => $orglogopath
						);
					
					
					$payload = json_encode($body);
					
					//print_r($payload);die;
					
					
					$msg = chr(0) . pack('n', 32) . pack('H*', $deviceToken) . pack('n', strlen($payload)) . $payload;
					
					
					$result = fwrite($fp, $msg, strlen($msg));
					
					if (!$result)
					{
						echo 'Message not delivered' . PHP_EOL;
					}
					else
					{
						echo 'Message successfully delivered' . PHP_EOL;
						$data=array('is_sent'=>'1');
						$this->db->where('in_id', $tid);
						$this->db->update('tbl_transport_animal_log', $data);
					}
					fclose($fp);
					//}
				}		
			}
		}
		
		else
		{
						$data=array('is_sent'=>'1');
						$this->db->where('in_id', $tid);
						$this->db->update('tbl_transport_animal_log', $data);
		}
	}	
	
