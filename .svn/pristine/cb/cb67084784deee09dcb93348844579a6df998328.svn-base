		$this->db->join("tbl_push_notification pn","pn.in_user_id = tu.in_user_id","LEFT");	
		//$this->db->where("pn.in_flg_trans_update",1);	
		//$this->db->where("tuv.in_deleted",'0');
		//$this->db->where("tu.in_deleted",'0');		
		//$this->db->where("tu.in_status <>",'3');
		
