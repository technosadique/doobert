@charset "utf-8";
/* CSS Document */

@import url('https://fonts.googleapis.com/css?family=Roboto:300,400,500,700');

* {
    -webkit-print-color-adjust: exact !important;   /* Chrome, Safari */
    color-adjust: exact !important;                 /*Firefox*/
}

body {font-family:"Roboto", Arial, sans-serif; color:#231F20; margin:0!important; padding:0!important;}

.br {clear:both; margin:0; padding:0;}
.center {text-align:center;}
.mar0 {margin:0;}

page { width: 21cm; /*21cm - 793.7007874*/ height: 28cm; /*29cm - 1096.0629921*/  } /*page[size="A4"]*/
page.h27 {height:27.5cm;}

/*.page1, .page2, .page3, .page4, .page5, .page6, .page7, .page8, .page9, .page10, .page11, .page12 {position:relative; overflow:hidden; padding:0;}*/
page {position:relative; overflow:hidden; padding:0; background:white; display:block; margin:0 auto; margin-bottom:0.5cm; box-shadow: 0 0 0.5cm rgba(0,0,0,0.5);}

p {font:400 16px/18px "Roboto", Arial, sans-serif; color:#414042; margin-bottom:9px;}

img {max-width:100%; height:auto;}
.break {page-break-after:always;}
table, th, td {border-collapse:collapse; padding:0; margin:0; text-align:left; border:none;}

.relative {position:relative;}

.pad25 {padding:0 0 25px 0;}

h2.orange {font-size:25px; color:#F05928; font-family:Roboto, Arial, "sans-serif"; font-weight:bold; font-style:normal; line-height:34px; margin:-34px 0 0!important; padding:0 0 16px!important; text-align:left;}
.fcorange {color:#F05928;}

.tabular {float:left; width:100%; margin:0;}
.tabular td {background:#fff; font:300 15px/18px "Roboto", Arial, sans-serif; color:#231F20; text-transform:none; text-align:left; padding:3px; border:none;}
.tabular td.w125 {width:125px;}
.tabular td.w136 {width:136px;}
.tabular td.w250 {width:250px;}
.tabular td a, .tabular td a:hover {color:#F05928; text-decoration:underline;}
.tabular td a.tel, .tabular td a:hover.tel {color:#231F20; text-decoration:none;}
.tabular td strong {font-weight:500;}

.stats .w75p {float:left; width:75%; margin:30px 0;}
.stats .w75p td {width:17%;}
.stats .w18p {float:right; width:18%; margin:30px 0;}
.stats td {background:#fff; font:300 15px/18px "Roboto", Arial, sans-serif; color:#231F20; text-transform:none; text-align:center; padding:5px 0 9px 0; border:none;}
.stats td a, .stats td a:hover {color:#F05928; text-decoration:underline;}
.stats td.grey {background:#F6F6F6;}
.stats td.number {font-size:22px; font-weight:500; padding-top:2px;}

.w33p {width:33%;} 

.crates {border:1px solid #A7A9AC; -webkit-border-radius:2px; -moz-border-radius:2px; -ms-border-radius:2px; -o-border-radius:2px; border-radius:2px; min-height:154px; margin:0 18px 18px 0; padding:10px 5px 10px 10px;}
.crates.marr0 {margin-right:0;}
.crates .w40p {width:40%; vertical-align:middle; border:1px solid #D1D3D4; text-align:center;}
.crates .w40p img {margin-top:3px;}
.crates .w60p {width:60%; vertical-align:top;}
.crates .w60p .padl16 {padding-left:16px;}
.crates .w60p h2 {font-size:18px; font-weight:bold; color:#F05928; margin:0; padding:0 0 5px 0;}
.crates .w60p h2 a {font-size:18px; font-weight:bold; color:#F05928;}
.crates .padt7 {padding-top:7px;}
.crates p {font-size:13px; font-weight:300; line-height:15px; color:#414042; margin:0; padding:0 0 2px 0;}
.crates p a {font-size:13px; font-weight:300; color:#414042; text-decoration:none;}

.comments {padding-top:25px;}
.comments td {color:#231F20; font-weight:400; font-size:16px; vertical-align:top;}
.comments td.number {color:#F05928; font-weight:bold; width:95px; padding-right:5px;}

.complete {border:1px solid #bdbec1; border-right:none; border-bottom:none;}
.complete th {background:#6D6E71; color:#fff; font-weight:400; padding:5px; border-right:1px solid #818386; font-size:12px; text-align:center;}
.complete td {background:#fff; color:#231F20; font-weight:300; padding:5px; border-right:1px solid #edeeef; border-bottom:1px solid #edeeef; font-size:12px; text-align:center;}
.complete td a {font-size:12px; color:#231F20; font-weight:300; text-decoration:none;}
.complete td.left {text-align:left;}
.complete tr.alternate td {background:#f1f1f2; border-color:#e3e4e5;}

@page {margin:0 22px 0 0;}

@media print {
page {box-shadow:none; margin:0!important; padding:0!important; size:landscape;}
@page rotated { size : landscape }
.pad25 {padding:43px 0 25px 25px;}
.ff .mz {height:110px;}
.ff page.mz {height:30cm; margin-top:61px;}
@page {size: landscape}
}

