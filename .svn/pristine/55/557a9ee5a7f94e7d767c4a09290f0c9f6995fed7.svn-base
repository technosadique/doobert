	public function create_ical() {
		
		$data = array();
		
		$data['all_events']	= $this->common_model->get_all_org_events($this->session->userdata('org_id'));
		
		$data['transport_events'] = $this->common_model->get_all_transportation($this->session->userdata('org_id'));
