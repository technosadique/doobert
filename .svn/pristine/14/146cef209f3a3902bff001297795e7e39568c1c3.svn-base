        if(amt){
          amount = amt;
        }else{        
          amount = jQuery(".other").val();

        }

        mode = jQuery('input[name=donate]:checked').val();
        if(mode){
          payment_type = mode;
        }      
     
      if(amount  != '' &&  payment_type != '')  {
       
          if(amount <= 0){
            alert("Donation amount should be greater than zero.");
            jQuery(".other").val('');
            return false;
          }

        jQuery("#donate_form").submit();
         return false;
        
      }else{
