function timeNow() 
{
	var d = new Date(),
      h = d.getHours(),
      m = d.getMinutes();
	  var timeString = h+':'+m+':'+'00';
	  var H = +timeString.substr(0, 2);
	  var h = (H % 12) || 12;
	  var ampm = H < 12 ? " AM" : " PM";
	  timeString1 = h + timeString.substr(2, 3) + ampm;
	  return timeString1
}

function verify_time()
{
	 var e = new Date();
	 return e.getHours()+':'+e.getMinutes()
}

