				}
			});
		} else {
			alert('You do not have a vehicle defined.');			
			if(typeof(veh_id) !== 'undefined' && veh_id!='' && veh_id!='0')	
			{	
				$('#manage_vehicle').val(veh_id);
			}
		}	
		
	});
	
