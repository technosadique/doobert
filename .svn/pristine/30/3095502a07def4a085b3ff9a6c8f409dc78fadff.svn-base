@font-face {
    font-family: 'swis721tnbtthin';
    src: url('../fonts/swis721/swis721tnbtthin.eot');
    src: url('../fonts/swis721/swis721tnbtthin.eot') format('embedded-opentype'),
         url('../fonts/swis721/swis721tnbtthin.woff2') format('woff2'),
         url('../fonts/swis721/swis721tnbtthin.woff') format('woff'),
         url('../fonts/swis721/swis721tnbtthin.ttf') format('truetype'),
         url('../fonts/swis721/swis721tnbtthin.svg#swis721tnbtthin') format('svg');
		 
}


