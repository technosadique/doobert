$(document).on('click', '#send_request_btn', function(){
	email_address = $('#user_email').val();
	user_name = $('#user_name').val();
	message = $('#contactmessage').val();
	var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
	var flag = 1;
	
	var error_message = '';
	
	if(user_name == '')
	{
		flag =0;
		error_message += 'Please enter name \n';
		//alert('Please enter Name');
	}
	
	if(email_address == '')
	{
		flag =0;
		error_message += 'Please enter Email address \n';
		//alert('Please enter Email address');
	}	
	if(!emailReg.test( email_address ))
	{
		flag =0;
		error_message += 'Please enter valid email address \n';
		//alert('Please enter validate Email address');
	}
	if(message == '')
	{
		flag =0;
		error_message += 'Please enter your message \n';
	}	
	
	
	if(flag == 1)
	{
		var transport_id 	= $('#transport_id').val();
		var contact_email    = $('#contact_email').val();
		var contact_name	= $('#contact_name').val();
		
			 $.ajax({
								type: "POST",
								url: '<?php echo base_url(); ?>open_leg/send_mail_to_tc',
								beforeSend: function(){
								$('#user-contact').modal('toggle');	
								//$.modal.close();
								//$('#user-contact').hide();
								//$.modal.close();
								$('#loader').show();
								},
								data: {
									user_name :user_name, 
									email_address :email_address ,
									contactmessage :message,
									transport_id :transport_id,
									contact_email :contact_email,
									contact_name : contact_name,	
									},
								dataType:'json',
			  					complete: function(){
								$('#loader').hide();
				    			  },
								  success:function(data){
									if(data.success == 1)
										$('.modal-title').html('<h4>Thank You</h4>')	
										$('#contactmessage').val('');
										$('#user_email').val('');
										$('#user_name').val('');
										$('#thankyou-config').modal('show');
								}, 
                        //return true;
						 });
		
	} else{ 
			alert(error_message);
	}	
});	
</script>
