.radio-btn input[type="checkbox"]:disabled+label, .radio-btn input[type="radio"]:disabled+label {color: #787878; cursor: default}
.radio-btn input[type="checkbox"]:disabled+label::after, .radio-btn input[type="radio"]:disabled+label::after {border: 3px solid #787878; background-color: #ffffff}
.radio-btn input[type="checkbox"]:disabled+label::before, .radio-btn input[type="radio"]:disabled+label::before {display: none}
.radio-btn label::after {-webkit-border-radius: 50%; border-radius: 50%; background-clip: padding-box}
.radio-btn label::before {width: 15px; height: 15px; border: none; top: 4px; left: 7px;	-webkit-border-radius: 50%;	border-radius: 50%;	background-clip: padding-box; background-color: #f35613; -moz-transform: rotate(0deg);	-o-transform: rotate(0deg);	-ms-transform: rotate(0deg); -webkit-transform: rotate(0deg);	transform: rotate(0deg)}

