					 $('#loader').hide();
				  },
				  success:function(data) {
					  //alert(data);
					  if(data != '') {
						  //$('#user-contact').modal('hide');
						  $('#contactmessage').val('');
						  $('#thankyou-config').modal('show');	
						  //alert('Your contact request was sent successfully.');
						  } else {
