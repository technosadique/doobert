		var map;
		var infoWindow;
		var bermudaTriangle;
		function initMap() {
    	<?php if(!empty($coordinates)) { ?>
		
		map = new google.maps.Map(document.getElementById('map'), {
        zoom: <?php echo $zoom ;?>,
        center: <?php if(!empty($center)) { echo $new ;} else { echo $lat[0];  };?>,
        mapTypeId: 'terrain'
        });
		var triangleCoords = [];
		var triangleCoords = [
		<?php echo $val ; ?>
		];
		
		<?php } else { ?>
		 map = new google.maps.Map(document.getElementById('map'), {
         zoom: 13,
         center: {lat: 41.875, lng: -87.642},
         mapTypeId: 'terrain'
        });
		var triangleCoords = [
		{lat: 41.875, lng: -87.642},
		{lat: 41.875, lng: -87.618},
		{lat: 41.886, lng: -87.631}
		];
		<?php } ?>
		// Construct the polygon.
        var bermudaTriangle = new google.maps.Polygon({
          paths: triangleCoords,
          strokeColor: '#FF0000',
          strokeOpacity: 0.8,
          strokeWeight: 3,
          fillColor: '#FF0000',
          fillOpacity: 0.35,
		
        });
        bermudaTriangle.setMap(map);
		bermudaTriangle.getPaths().forEach(function(path, index){

  google.maps.event.addListener(path, 'insert_at', function(){
	   var vertices = bermudaTriangle.getPath();
	   var contentString = [] ;
       
        for (var i =0; i < vertices.getLength(); i++) {
          var xy = vertices.getAt(i);
          contentString +=  '/{lat:'+ xy.lat() + ', lng:' + xy.lng()+' } ';
		 }
		 
		infoWindow.open(map);
    
  });

  google.maps.event.addListener(path, 'remove_at', function(){
	 
  });

  google.maps.event.addListener(path, 'set_at', function(){
	   var vertices = bermudaTriangle.getPath();
	   var contentString = [] ;
        
       for (var i =0; i < vertices.getLength(); i++) {
          var xy = vertices.getAt(i);
          contentString +=  '/{lat:'+ xy.lat() + ', lng:' + xy.lng()+' } ';
		}
		
		infoWindow.open(map);
   
	});

	});
	 infoWindow = new google.maps.InfoWindow;
}

</script>
