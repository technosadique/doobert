	function search_photo_type(pgtypeval) {  
		
		var searchproduct 	= jQuery('.search_product_value').val();
		var order 			= jQuery('#order').val();
		var currpage 		= jQuery('#currpage').val();
	   
	  var currt_id          = jQuery('#currt_id').val();
	   
	   var checkboxValues = [];		
		
		
		
		var product_type = [];
		
		
		var newpageno	=	0;
		if(pgtypeval==''){					
			jQuery('#currpage').val('0');			
		}else{
			newpageno = parseInt(currpage)+1;
			jQuery('#currpage').val(newpageno);
		}	
		
		urlval= "<?php echo $base_url.'user/ajax_photo_list'; ?>"
		//console.log(urlval);
		//console.log('your message');
		jQuery.ajax({
			type: "POST",
			dataType: "json",
			async: false,
			url: urlval,
			data: {currpage:newpageno,currt_id:currt_id},
            success: function(result)
            {   console.log(result);
			    if(result.results == false && $('.product-img').length<=0)
				{   jQuery('.ajax-product').empty();
				    $('#ajaxLoad').hide();
					jQuery('.ajax-product').html("<span style='text-align:center;font-size:16px;'>No photo(s) found.</span>");
					return false;
				}
				
			if(result.results != false)
			{	
			if(result.results.all_photos.length > 0 )
			{	
			for(var i=0; i<result.results.all_photos.length; i++){
				if(result.results.all_photos[i].p_username == null){ result.results.all_photos[i].p_username = ''; }
				var row ='<li style="min-height:1px;">';                   
					row +='<div class="product-img"><a href="<?php echo $base_url; ?>upload/trasnportation/'+result.results.all_photos[i].p_filename+'"  rel="prettyPhoto[mixed]"><span class="play-icon"><img src="<?php echo $base_url; ?>/upload/trasnportation/250x250/'+result.results.all_photos[i].p_filename+'" /></span></a></div><span><p >'+result.results.all_photos[i].p_username+'</p></span>';
					//row +='<div class="product-img"><a href="<?php echo $base_url; ?>upload/trasnportation/'+result.results[i].p_filename+'"  rel="prettyPhoto[mixed]"><span class="play-icon"><img src="<?php echo $base_url; ?>/upload/trasnportation/250x250/'+result.results[i].p_filename+'" /></span></a></div><span><a href="'+result.results[i].p_userprofile+'" target="_blank">'+result.results[i].p_username+'</a></span>';
					row +='<div class="clearfix"></div>';				
				row +='</li>';
					
			   //alert(row);
			  jQuery('.ajax-product').append(row);
			  
			    $("area[rel^='prettyPhoto']").prettyPhoto();
				$(".gallery:first a[rel^='prettyPhoto']").prettyPhoto({animation_speed:'normal',theme:'light_square',slideshow:3000, autoplay_slideshow: false,  changepicturecallback: function(){add_download()}});
				//$(".gallery:first a[rel^='prettyPhoto']").prettyPhoto({animation_speed:'normal',theme:'light_square',slideshow:3000, autoplay_slideshow: false});
				$(".gallery:gt(0) a[rel^='prettyPhoto']").prettyPhoto({animation_speed:'fast',slideshow:10000, hideflash: true});
			   
				}
			}
			 
			// SHOW HIDE LOAD MORE BUTTON
			if(result.results.total_count > result.results.all_photos.length)
					{	
						jQuery('#dobt-pagination').html('<a href="javascript:void(0);" id="ajaxLoad" class="loadmore">Load More</a>'); 
					} else {
						jQuery('#dobt-pagination').html('');
					}
			/*if(result.results.all_photos.length > 0 && result.results.total_count!='0' && result.results.all_photos.length<result.results.total_count)
			{	
				jQuery('#dobt-pagination').html('<a href="javascript:void(0);" id="ajaxLoad" class="loadmore">Load More</a>'); 
			}*/
			}	else {
				jQuery('#dobt-pagination').html('');	
			}	
			
			
            }, error: function(){
			//alert('!! No Record Found !!');
			}
           
       });   
	   
	}		
