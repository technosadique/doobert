<script> 
 //Added By Nirav 23 May 2016 
function update_login_status_direct(ajaxurl,redirecturl,status)
		{
			$.ajax({
				type:'POST',
				url:ajaxurl,
				data:{
					'status':status,
				},
				success: function(result){
						if(result === 'true')
							window.location = redirecturl;
						else
							alert("Login status direct is not enabled");
				},		
			});
		}
</script>