function timeConvertor(time) 
{
    var PM = time.match('PM') ? true : false
    
    time = time.split(':')
    var retrive_min = time[1];
    var min = retrive_min.split(" ");
	var minutes = min[0];
    if (PM && time[0]!='12') 
	{
        var hour = 12 + parseInt(time[0],10)
        var sec = '00';
    } else {
        //var hour = time[0]
		//var hour = time[0] > 9 ?time[0]: "0" + time[0];
		var hour = time[0] <= 9 && time[0].length<=1 ?"0" + time[0]:time[0];
        var sec = '00';       
    }
	var exact_time = hour + ':' + minutes + ':' + sec;
    return exact_time;
}
function get_month(months)
{
	switch (months) {
    case 'Jan':
    month = "01";
    break;
    case 'Feb':
    month = "02";
    break;
    case 'Mar':
    month = "03";
    break;
	case 'Apr':
    month = "04";
    break;
    case 'May':
    month = "05";
    break;
    case 'Jun':
    month = "06";
    break;
	 case 'Jul':
    month = "07";
    break;
    case 'Aug':
    month = "08";
    break;
    case 'Sep':
    month = "09";
    break;
	case 'Oct':
    month = "10";
    break;
    case 'Nov':
    month = "11";
    break;
    case 'Dec':
    month = "12";
}
return month;
}

function verify_date_time()
{
	var latest_date_time = '0';
	var today 		 = new Date();
	var start_date   = $('#transportdate').val();
	var s_date 	     = start_date.split(" ");
	//var day          = s_date[2];
	//var day          = s_date[2] > 9 ?s_date[2]: "0" + s_date[2];
	var day          = s_date[2] <= 9 && s_date[2].length<=1 ?"0" + s_date[2]:s_date[2];
	var month        = get_month(s_date[1]);
	//var year         = today.getFullYear();
	var year         = $('#transportdate_year').val();
	var time_picker1 = $('#timepicker1').val();
	var s_time       = timeConvertor(time_picker1);
	var s_date_time  = year+'-'+month+'-'+day+'T'+s_time;
	var end_date     = $('#to_transportdate').val();
	var e_date 	     = end_date.split(" ");
	//var e_day        = e_date[2];
	//var e_day        = e_date[2] > 9 ?e_date[2]: "0" + e_date[2];
	var e_day        = e_date[2] <= 9 && e_date[2].length<=1 ?"0" + e_date[2]:e_date[2];
	var e_month      = get_month(e_date[1]);
	//var e_year       = today.getFullYear();
	var e_year       = $('#to_transportdate_year').val();
	var time_picker2 = $('#timepicker2').val();
	var e_time       = timeConvertor(time_picker2);
	var e_date_time  = e_year+'-'+e_month+'-'+e_day+'T'+e_time;
	var pickDate 	 = new Date(s_date_time);
	var droffDate    = new Date(e_date_time);
	if (pickDate > droffDate)
	{
		latest_date_time = '1';
	}
	return latest_date_time;
}

