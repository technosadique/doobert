		$(document).on('change','#dropoff_location',function(){	
			var str = document.getElementById('dropoff_location').value;
			var zip_code_d = document.getElementById('postal_code_d').value;
			if(str!='')
			{
					$('#loader').show();
					$.ajax({
						type: "POST",
						async: false,
						url: base_url+"org_transportation/get_ride_address/",
						dataType: "json",
						data:
						{
							term: str,
						},
						success: function (data)
						{
							$('#loader').hide();
							if(data.status == 'ok')
							{
								$("#postal_code_d").val(data.postal_code);
								$("#locality_d" ).val(data.city);
								$("#administrative_area_level_1_d" ).val(data.state);
								$('#street_number_d').val(data.street_address);
								document.getElementById('route_d').value = '';
								//$("#dropoff_zip").val(data[0].value);
							}
							else
							{
									//alert('Please enter valid Street Address, City, State, Zip Code location');	
									$('#dropoff_location').val('');
									document.getElementById('postal_code_d').value = '';
									document.getElementById('street_number_d').value = '';
									document.getElementById('route_d').value = '';
									document.getElementById('locality_d').value = '';
									document.getElementById('administrative_area_level_1_d').value = '';
									document.getElementById('country_d').value = '';
								
							}
						}
					});
				
			} else {
				document.getElementById('postal_code_d').value = '';
				document.getElementById('street_number_d').value = '';
				document.getElementById('route_d').value = '';
				document.getElementById('locality_d').value = '';
				document.getElementById('administrative_area_level_1_d').value = '';
				document.getElementById('country_d').value = '';
				if(toloc_error_class=='1')
				{	
					$('#dropoff_location').parent('.form-group').addClass('has-error');
					$('#dropoff_location').next('.help-inline').show();
				}
			}		
		});
		
