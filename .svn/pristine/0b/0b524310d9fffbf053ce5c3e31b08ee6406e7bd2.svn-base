<script>
function isNumber(evt) {
    evt = (evt) ? evt : window.event;
    var charCode = (evt.which) ? evt.which : evt.keyCode;
    if (charCode > 31 && (charCode < 48 || charCode > 57)) {
        return false;
    }
    return true;
}

function autosave(orgid)
	{	
		base_url='<?php echo base_url();?>';		
		$.ajax({	
			type: "POST",			
			url: base_url+'registration/add_org_user',
			data: {orgid:orgid}, 						
			dataType:"json",
			success:function(data) 
			{   ///console.log(data);
				//alert(data.success);
				if(data.success == 2){
				$("#div_"+orgid).remove();
				}
				
			}
		});	
	
	}
</script>
