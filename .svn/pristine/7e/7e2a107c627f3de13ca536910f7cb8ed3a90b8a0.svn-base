		if(tipsspan.length)
		{
		var tipofset = tipsspan.offset().top;
				
		var overflowtips = tipofset - 106;
		
		if(tableheight < overflowtips){
			$(this).find('span').css('top', - tipsspanheight);
			$(this).find('.arrow').css('top', tipsspanheight+5);
		}
		}
		//End removing top height
		
	});
	
