<script type="text/javascript">

$(document).ready(function(){
	
	$(".dismiss_tooltip").live("click",function(){	
		//alert("d");
		$.setCookie("tooltip","Y",90);
		$(".popup-box").hide(300);
	});	
	$.setCookie = function(cname,cvalue,exdays) {
	 	var d = new Date();
		d.setTime(d.getTime() + (exdays*24*60*60*1000));
		var expires = "expires=" + d.toGMTString();
		//document.cookie = cname+"="+cvalue+"; "+expires;
		document.cookie = cname+"="+cvalue+"; "+expires+"; path=/";
	};	
	
	$('#loadimg').hide();
	$("#address").focus();
	
		
	$('#submit').on('click',function(){
		
		var boxes = $('.roles:checkbox');
		if(boxes.length > 0) {
			if( $('.roles:checkbox:checked').length < 1) {
				alert('Please select at least one checkbox');
				boxes[0].focus();
				return false;
			}
			else {
				$('#submit').hide();
				if($.browser.msie) {
					$('.display_loader').html('<img id="loadimg" src="<?php echo base_url(); ?>assets/img/ajax-loading.gif" alt="loader" />');
				}
				$('.display_loader').show();
				$('#loadimg').show();
				$('#my_profile').submit();
			}
		}
		 		
	});
	
	$('.roles').on('click',function(){
		 if (!$(this).is(':checked')) {
			if($(this).attr("id") != "social_media_other" || $(this).attr("id") != "animal_profile_other")
			{
				$(this).closest('.unselect_grey').find('.roles_select').val('1');
			}
			$(this).closest('.unselect_grey').toggleClass('virtual_grey');
			$(this).closest('.unselect_grey').find('.roles_select').attr('disabled', true);			 
			if($(this).attr("id") == "social_media_other")
			{
				$("#social_media_other_detail").val("");
			}
			if($(this).attr("id") == "animal_profile_other")
			{
				$("#animal_profile_other_detail").val("");
			} 
		}else{
			$(this).closest('.unselect_grey').toggleClass('virtual_grey');
			$(this).closest('.unselect_grey').find('.roles_select').attr('disabled', false);			 
			}	
	});

});
//

function removeVirtualVolunteer()
{ 		
	 if(confirm('Are you sure you want to delete this record?'))
	 {	 
	 	$('.roles').prop('checked', false);
		$('.roles_select').val('1');
		$("#social_media_other_detail").val("");
		$("#animal_profile_other_detail").val("");
		$('.remove_select').parent('div').addClass('virtual_grey');
		$('.roles_select').attr('disabled', true);
		$(":checkbox").uniform({radioClass: 'myRadioClass'});
		jQuery.post("<?php echo base_url(); ?>user/delete_virtual_volunteer/",{action:"deleted"}, function( data ) {
		jQuery("#error_ajax").html(data);
		$(".edit-links").hide();
		});
	 }
	 else
	 {
		 return false;
	 }	
}
