/* CSS Document */
@charset "utf-8";
@import url(http://fonts.googleapis.com/css?family=Open+Sans:400,800,700);
img {behavior: url(pngbehavior.htc);}

* {margin:0; padding:0;}

body.page-general-infographic {font:12px 'OpenSansRegular', Verdana, Arial, Helvetica, sans-serif; color:#392a0f; background:url(../images/body-paw.gif);}

.page-general-infographic h1, .page-general-infographic h2, .page-general-infographic h3, .page-general-infographic h4, .page-general-infographic h5, .page-general-infographic h6, .page-general-infographic p, .page-general-infographic ul, .page-general-infographic li {margin:0; padding:0; list-style:none; font-weight:normal;}
.page-general-infographic a:focus{outline:none;}

.page-general-infographic img {border:none;}
.page-general-infographic .br, .page-general-infographic .clear {clear:both; margin:0px; padding:0px;}

.page-general-infographic .margin {margin:0px;}

/* ------------------ Wrap --------------------*/
.page-general-infographic #outer_wrap{width:100%;}

