        var url = base_url+"partner/remove_favorite/"+org_id;           
        $.ajax(
            {
                url: url,
                dataType: "json",
                method:"post",
                data:{
                    org_id:org_id
                },
                success: function (data)                
                {
                    if(data.res == 'success'){                      
                        window.location.reload();
                    }                           
                }
        });
       

    }
	
	function add_favorite(ele){
	var org_id = $(ele).data('value');
