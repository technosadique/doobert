ol {
  font-size: 17px;
  line-height: 28px;
  counter-reset: li;
  list-style: none;
  margin: 10px 0 35px 0;
  padding: 0;
  width: 100%;
  display: block;
  float: left;
  clear: both;
}
ol > li {
  margin-bottom: 10px;
  padding-left: 40px;
  line-height: 20px;
  position: relative;
}
ol > li:before {
  color: #a6acac;
  font-family: 'Museo Slab W01', serif;
  font-weight: 200;
  font-size: 17px;
  line-height: 17px;
  text-align: right;
  content: counter(li) ". ";
  counter-increment: li;
  margin: 0px;
  width: 30px;
  height: 16px;
  display: block;
  position: absolute;
  top: 0px;
  left: 0px;
}
ol > li > a {
  color: #c29400;
  border-bottom: 2px rgba(194, 148, 0, 0.3) dotted;
}
ol > li > a:before {
  content: "";
  position: absolute;
  width: 100%;
  height: 2px;
  bottom: -2px;
  left: 0;
  background-color: #d6a400;
  visibility: hidden;
  -webkit-transform: scaleX(0);
  transform: scaleX(0);
  -webkit-transition: all 0.3s ease-in-out 0s;
  transition: all 0.3s ease-in-out 0s;
}
ol > li > a:focus,
ol > li > a:hover {
  color: #d6a400;
}
ol > li > a:focus:before,
ol > li > a:hover:before {
  visibility: visible;
  -webkit-transform: scaleX(1);
  transform: scaleX(1);
}
.btn {
