		class tagCloud{
			private $tagsArray;
			public function __construct($tags)
			{
			 /*** set a few properties ***/
			 $this->tagsArray = $tags;
			}

			public function displayTagCloud()
			{
			 $ret = '';
			 shuffle($this->tagsArray);
			 
			 foreach($this->tagsArray as $key=>$tag)
				{
						if($key%2 == 0)
						{
						$ret.='<span style="font-size: '.$tag['weight'].'px; color: black;text-decoration: none;text-transform: capitalize;" >'.$tag['tagname'].'</span>'."\n";
						}
						else
						{
						$ret.='<span style="font-size: '.$tag['weight'].'px; color: #00a4a7;text-decoration: none;text-transform: capitalize;" >'.$tag['tagname'].'</span>'."\n";
						}
						
						
						
						
				}
			 return $ret;
			}
				

