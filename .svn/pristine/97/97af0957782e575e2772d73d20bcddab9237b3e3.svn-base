function get_vaccin(get_id) {
	if(get_id == "recent-vaccns-yes") {
		if($('#'+get_id).closest('span').hasClass('checked')){
			$('.vaccin_datepicker_block').show();
			$('.vaccin_date').show();
			$('#recent_vaccin_date').val('vaccin_date');
		} else {
			//$('.vaccin_date').hide();
			$('.vacc_date').hide();			
			$('.vaccin_datepicker_block').hide();
			$('#recent_vaccin_date').val('');
		}
	} else {
		$('.vaccin_date').hide();
		$('.vaccin_datepicker_block').hide();
		$('#recent_vaccin_date').val('');
	}
}

function get_cvi(get_cvi_id) {
	if(get_cvi_id == "cvi_yes") {
		if($('#'+get_cvi_id).closest('span').hasClass('checked')){
			$('.cvi_datepicker_block').show();
			$('.cvi_date').show();
			$('#cvi_date_val').val('cvi_date_val');
		} else {
			$('.cvi_date').hide();
			$('.cvi_datepicker_block').hide();
			$('#cvi_date_val').val('');
		}
	} else {
		$('.cvi_date').hide();
		$('.cvi_datepicker_block').hide();
		$('#cvi_date_val').val('');
	}
}

function show_hide_fields(animal_type) {
	if(animal_type == 4) {//Dogs
		$("#lbl_create_well").html("Crates Well?:");
		$('.dogs_label').html('Good with other dogs?:');
		$('.cats_label').html('Good with cats?:');
		$('.interchange').addClass('dog-flr');
		
		$('#animal-content_wrap').show();
		
		$('.recent_vaccin').show();
		$('.house').show();
		$('.dogs').show();
		$('.kids').show();
		$('.cats').show();
		$('.flight_risk').show();
		$('.cvi').show();
		$('.barrier').show();
		$('.crates').show();
		$('.foster_title').show();
		
		$('.declawed').hide();
	} else if(animal_type == 3) {//Cats
	
		$("#lbl_create_well").html("Crates Well?:");
		$('.dogs_label').html('Good with dogs?:');
		$('.cats_label').html('Good with other cats?:')
		$('.interchange').removeClass('dog-flr');
			
		$('#animal-content_wrap').show();
	
		$('.recent_vaccin').show();
		$('.declawed').show();
		$('.dogs').show();
		$('.kids').show();
		$('.cats').show();
		$('.flight_risk').show();
		$('.cvi').show();
		$('.barrier').show();
		$('.crates').show();
		$('.foster_title').show();
		
		$('.house').hide();
	} else if(animal_type == 1 || animal_type == 5 || animal_type == 6) {//Horse Barnyard Other
		if(animal_type == 5){//Horse
		$("#lbl_create_well").html("Trailers Well?:");
		}
		else{ //Barnyard Other
			$("#lbl_create_well").html("Trailers/Crates Well?:");
		}
	
		$('#animal-content_wrap').show();
		$('.recent_vaccin').show();
		$('.cvi').show();
		$('.barrier').show();
		$('.crates').show();
		
		$('.foster_title').hide();
		$('.flight_risk').hide();
		$('.house').hide();
		$('.declawed').hide();
		$('.dogs').hide();
		$('.kids').hide();
		$('.cats').hide();
	} else {
		$('#animal-content_wrap').hide();
	
		$('.recent_vaccin').hide();
		$('.declawed').hide();
		$('.dogs').hide();

		$('.kids').hide();
		$('.cats').hide();
		$('.flight_risk').hide();
		$('.cvi').hide();
		$('.barrier').hide();
		$('.crates').hide();
		$('.foster_title').hide();
		
		$('.house').hide();
	}
}
