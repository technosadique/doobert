}

#wt-slider .loading .flex-viewport {
    max-height: 300px;
}

#wt-slider .attachment-wt-slider-image {
    height: 310px;
    width: 630px;
}

#wt-slider .slides li {
    position: relative;
}

#wt-slider .slider-text {
    background: #333333;
	height: 80px;
