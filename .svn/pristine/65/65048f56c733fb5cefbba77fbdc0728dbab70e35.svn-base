	overflow: hidden;
	margin-left: 25px;
}

#top-stories .box:first-child{
	margin-left: 0;
	width: 207px;
}

#top-stories .header {
	background: #272727;
	color: #FFFFFF;
	font-weight: normal;
	padding-left: 0;
	width: 180px;
	text-align: right;
}

