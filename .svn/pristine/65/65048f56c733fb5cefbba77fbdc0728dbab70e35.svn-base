 /* 1. BASE
 * ================= */	
body{
	font-family: "Open Sans","Tahoma","Verdana","Arial", sans-serif;
    font-size: 14px;
    font-weight: 400;
	line-height: 1.7;
	color: #535353;
}

h1, h2, h3, h4, h5, h6 {
	font-family: "Open Sans","Tahoma","Verdana","Arial", sans-serif;
	color:#444;
	line-height: 1.35;
}

h1{
    font-size: 30px;
	font-weight: 600;
	line-height: 36px;    
	margin-bottom: 5px;
}

h2 {
    font-size: 20px;
    font-weight: 600;
	line-height: 40px;
}

h3 {
    font-size: 16px;
    font-weight: 600;
    line-height: 27px;
}

h4 {
    font-size: 14px;
    font-weight: 600;
}

h5 {
    font-size: 14px;
    font-weight: 400;
}

h6 {
    font-size: 13px;
	text-transform: uppercase;
    font-weight: 400;
	color: #999999;
}

a:link {
	color: #464646;
	-moz-transition:  color 0.3s linear 0s;
	-webkit-transition:  color 0.3s linear 0s;
	transition:  color 0.3s linear 0s;	
	text-decoration:none;
}
a:visited {
	color: #464646;
} 

a:hover{
	color: #c51e52;/*359BED*/
}

:focus {
	outline: 0;
}

textarea{ resize:none;}

#container{
	width: 1240px; 
	margin: 0 auto;	
	/*overflow: hidden;*/
}

#main{
	overflow:hidden;	
}

#content {
	width: 864px;/*640px; 28Apr15*/			
	float: left;
	margin-right: 27px;	
	overflow: hidden;
}/*margin-right: 28px;*/

.page-id-286 #content {width: 705px; margin-right: 0px;	}

#content.post {width:864px;}/*940px;*/

#left-sidebar{
	float: left;
	margin-right: 30px;	
	overflow: hidden;
}

#right-sidebar{	
	float: left;	
	overflow: hidden;
}

.sidebar{
	width: 348px;/*width: 345px;*/
}

.onesidebar-page{
	width: 865px !important;	
}

.full-content{
	width: 100% !important;
}

.one-half{
	width: 305px;	
	margin-right: 30px;
	float: left;
}

.last-col{
	margin-right: 0;
}

.wp-post-image{
	display: block;
}

.lb-image{ 
	max-width: inherit; 
}

input, textarea, button {
    /*-webkit-appearance: none;*/ /*Safari/Chrome*/   /*Hide beacuse of poll radio not showing on chrome and safari*/
    -moz-appearance: none; /*Firefox*/
    -ms-appearance: none; /*IE*/
    -o-appearance: none; /*Opera*/
    appearance: none;

    -webkit-border-radius: 0; 
}

/* 2. HEADER
* ============== */	
#header{
	margin: 0px auto; 
	background: #ffffff; 
	margin:17px 0 30px 0; /*30px 0 - 26dec14*/
	position: relative;
}

#header .wrap{
	overflow: hidden;
}

#header .logo{
	float: left;
	display: inline-block;
	/*width: 392px; height: 55px;*/
	margin-right: 30px;/*64px 28Apr15*/ /*30px 26dec14*/
	overflow: hidden;
}

#header .logo h1{
	margin-bottom: 0px;
	display: inline-block;
	line-height: 1;
	font-size: 48px;
}

#header .logo img{
	display: block;
}

#header .date{
	width: 250px;
	color: #AEAEAE;
	float: left;
	margin-right: 30px;
	margin-top: 15px;
	font-size: 14px;
}

#header .search{
	width: 450px; /*300px 26dec14*/
	float: right;/*float: left;*/
	/*margin-right: 30px;*/
	padding:20px 14px;
	background:#f3f3f3;

}

#header .social{
	max-width: 207px;
	float: right;	
}

#header .social ul{
	list-style: none;
	margin: 0;
	padding: 0;	
}

#header .social ul li{
	float: left;
	margin-left: 5px;	
}

#header .social ul li a{
	display: block;
	float: left;
	height: 48px;
	width: 48px;
	text-indent: -999em;   
}

#header .social ul li:first-child{
	margin-left: 0;
}

#header .social ul li a{
	-webkit-transition: all 0.3s; 
	-moz-transition: all 0.3s; 
	transition: all 0.3s;
}

#header .social ul li a:hover{
	opacity: 0.8;
}

#header .social ul li a.twitter{
  background: url("images/twitter.png") no-repeat left center;  
}

#header .social ul li a.fb{
  background: url("images/facebook.png") no-repeat left center;  
}

#header .social ul li a.gplus{
  background: url("images/gplus.png") no-repeat left center;  
}

#header .social ul li a.rss{
  background: url("images/rss.png") no-repeat left center;  
}

/* 3. NAVIGATION MENU
 * ======================== */
/*-- Main Menu --*/
#main-menu {
	width: 100%;
	background: #333333;
	float: left;
	position: relative;
	z-index: 30;
	margin-top: 17px; /*30px 26dec14*/
	height: 47px;	
}

#main-menu select {
    display: none;
    z-index: 3000;
    position: relative;
    padding: 6px;
    font-size: 14px;
    border: 1px solid #EEEEEE;
    height: 34px;	
	color:#555555;
	font-family:inherit;	
}

#main-menu ul {
	float:left;
	list-style-type:none;
	margin:0px;
	padding:0px;
	position:relative;
	z-index:99;
}

#main-menu ul li {
	float:left;	padding:0px;
	margin:0px;
	position:relative;
}

#main-menu ul li a {
	display: block;
	padding: 13px 22px 12px;
	text-align: center;
	font-size: 15px;
	text-transform:uppercase;
	font-weight: 600;
	line-height: 22px;
	color: #FFFFFF;	
}

#main-menu .current-menu-ancestor, #main-menu .current-menu-item, #main-menu .current-menu-parent,
#main-menu .current_page_item, #main-menu ul li a:hover, .archive.category #menu-item-644, .single.single-post #menu-item-644{
	background: #ec6d36;
}


#main-menu ul li ul {
	display:none;
	position:absolute;
	padding:0;
	background: #F2F2F2;
	margin-top: 0; border-top:2px solid #c51e52; z-index:9999;
}

#main-menu ul li ul li {
	float:none;
	padding:0px;
}

#main-menu ul li ul li a, 
#main-menu ul li:hover ul li a, 
#main-menu ul li.over ul li a {	
	padding: 11px 20px 10px 21px;	/*12px 20px 12px 18px;	*/
	display: block;	
	width: 220px;	
	text-align: left;	
	height: auto;	
	background: #eee;/*EEEEEE*/		
	font-size: 14px;	
	font-weight: 400;	
	border-bottom: 1px solid #dedede;/*ccc*/	
	color: #333333;	
	text-transform: none; font-weight:600;
}/*width: 142px;*/

#main-menu ul li ul li:hover a, 
#main-menu ul li ul li.over a, #main-menu ul li ul li.current-menu-item a, #main-menu ul li ul li.current_page_item a {color: #c51e52; font-weight:600;}

#main-menu .sub-menu ul li.current_page_item a {color: #fff !important; font-weight:600;}/*color: #c51e52;*/

#main-menu ul li ul li ul {	
	display: none;	
	position: absolute;	
	left: 180px;	
	top: -1px;	
}

#main-menu ul li ul li:hover ul, 
#main-menu ul li ul li.sfHover ul {	
	display: block;	
	position: absolute;	
	left: 180px;	
	top: 0;	
}

#main-menu ul li ul li:hover ul li a, 
#main-menu ul li ul li.over ul li a {	
	color: #000000;	
}

#main-menu ul li ul li ul li a:hover {
	color: #c51e52;
}/*color: #000000;*/

#main-menu a.sf-with-ul {
	padding-right:25px;
	min-width:1px;
}

#main-menu .sf-sub-indicator {
	display: none;
}

#main-menu a.menu-link {
	display: none;
}

#main-menu .menu-item-home{
	width: 60px;
}

#main-menu .menu-item-home a{
	background: url("images/home.png") no-repeat 50% 50%;
	text-indent: -999em;
	display: block;	
}

#main-menu .menu-item-home a:hover{
	background: #ec6d36 url("images/home.png") no-repeat 50% 50%;
}

 /* 4. CONTENT
 * ==================== */
#top-stories {
	overflow: hidden;
	margin-bottom: 30px;
	margin-top: 20px;
}

#top-stories .thumb-wrap{  
	position: relative;
	overflow: hidden;
	width: 75px;
