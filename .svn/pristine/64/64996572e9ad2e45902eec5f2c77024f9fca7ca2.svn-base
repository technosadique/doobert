body.hmbody {
	background:#0891c2 url(../images/layer.png) repeat;
	color:#FFFFFF;
}
body.hmbody a {	outline:none;text-decoration:none;color:#FFFFFF;}
body.hmbody a:hover {text-decoration:none;}
body {
	margin:0;
	padding:0;
	background:#ffffff;
	font:400 16px/20px 'Open Sans',Helvetica,Arial,sans-serif;
	color:#444444;
	overflow-x:hidden;
}

.clearfix {clear:both;margin:0;padding:0;}
a {	outline:none;text-decoration:none;color:#444;}
a:hover {text-decoration:none;}

::-moz-selection { background:#9b9b9b; color:#ffffff; text-shadow:none}
::selection { background:#9b9b9b; color:#ffffff; text-shadow:none}

::-webkit-input-placeholder { color:#333333;}
:-moz-placeholder {color:#333333;} /* Firefox 18- */ 
::-moz-placeholder { color:#333333;} /* Firefox 19+ */
:-ms-input-placeholder { color:#333333;}

.text-alignr {text-align:right;}
img { border:none;}
h1, h2, h3, h4, h5, h6, p {margin:0;padding:0;}
ul {margin:0;padding:0;}
span.brkline { display:block;}
span.underline{text-decoration:underline;}
.fbpage-wrap {width:100%;margin:0;padding:0;}
.fbpage {margin:0 auto;padding:0;max-width:1135px;}


/*header start*/
.header {margin:0;padding:0;width:100%;background:#fff;height:39px;padding:8px 0}
.header .logo {float:left;width:130px;padding-left:8px;}
.header .invite-btn-wrap {margin:0 auto;width:240px;}
.header .invite-btn-wrap a {background:#8dbf16;padding:5px 14px;font:600 16px/35px 'Open Sans',Helvetica,Arial,sans-serif;border-bottom:3px solid #60a003;-webkit-border-radius:3px;-moz-border-radius:3px;border-radius:3px;color:#fff;}
.header .invite-btn-wrap a:hover {background:#60a003;}
.header .fblink-wrap {float:right;width:auto;padding-top:8px;padding-right:15px;}

.fbpage .top-menu-wrap {background:url(../images/bg_trans.png) repeat;padding:11px;width:580px;margin:0 auto;-webkit-box-shadow:0px 2px 10px 0px rgba(0, 78, 122, 0.29);-moz-box-shadow:0px 2px 10px 0px rgba(0, 78, 122, 0.29);box-shadow:0px 2px 10px 0px rgba(0, 78, 122, 0.29);border-bottom-left-radius:3px;border-bottom-right-radius:3px;}
.fbpage .top-menu-wrap ul li {list-style:none;display:inline-block;}
.fbpage .top-menu-wrap ul li a {float:left;background:#df1051;padding:10px;margin-right:9px;-webkit-border-radius:3px;-moz-border-radius:3px;border-radius:3px;font-weight:600;}
.fbpage .top-menu-wrap ul li a.last {margin:0;}
.fbpage .top-menu-wrap ul li a:hover {background:#c50441;}
.fbpage .rescue-text {text-align:center;}
.fbpage .rescue-text h2 {font:700 25px/32px 'Open Sans',Helvetica,Arial,sans-serif;margin:32px 0 5px;padding:0}
.fbpage .rescue-text p {font:400 16px/20px 'Open Sans',Helvetica,Arial,sans-serif;}
.fbpage .center-section {width:100%;margin:25px 0 40px;padding:0 0 19px;border-bottom:1px solid #95d0e5;}
.fbpage .box {float:left;width:273px;height:96px;background:#fff;-webkit-border-radius:3px;-moz-border-radius:3px;border-radius:3px;color:#004e7a;text-align:center;padding:18px 0;border-bottom-right-radius:0;-webkit-box-shadow:0px 2px 10px 0px rgba(0, 78, 122, 0.29);-moz-box-shadow:0px 2px 10px 0px rgba(0, 78, 122, 0.29);box-shadow:0px 2px 10px 0px rgba(0, 78, 122, 0.29);}
.fbpage .box p.title {font:700 23px/25px 'Open Sans',Helvetica,Arial,sans-serif;padding-bottom:10px;}
.fbpage .box .view-animal {background:#fe4d00;display:inline-block;color:#fff;padding:0 20px;font:700 16px/36px 'Open Sans',Helvetica,Arial,sans-serif;-webkit-border-radius:3px;-moz-border-radius:3px;border-radius:3px;}
.fbpage .box .right-corner {background:url(../images/right_corner.png) no-repeat right bottom;float:right;height:57px;margin-right:-24px;width:25px;}
.fbpage .center-section .donate {float:left;width:524px;padding:0 7px 0 30px;}
.fbpage .center-section .centerbg {float:left;width:13px;margin-bottom:-44px;}
.fbpage .center-section .centerbg img {margin-top:11px;}
.fbpage .center-section .rescue {float:left;width:523px;padding-right:30px;}
.fbpage .center-section .rescue .box {float:right;border-bottom-left-radius:0;border-bottom-right-radius:3px;}
.fbpage .box .left-corner {background:url(../images/left_corner.png) no-repeat left bottom;float:left;height:59px;margin-left:-25px;width:25px;}
.fbpage .rescue-pic {padding-left:10px;}
.fbpage .center-menu-wrap {border-bottom:1px solid #95d0e5;padding-bottom:0px;text-align:center;}
.fbpage .center-menu-wrap ul {margin:0 0 18px;}
.fbpage .center-menu-wrap ul li {list-style:none;display:inline-block;}
.fbpage .center-menu-wrap ul li a {background:#046c9a;padding:0 20px;line-height:42px;display:block;-webkit-border-radius:3px;-moz-border-radius:3px;border-radius:3px;margin-right:20px }
.fbpage .center-menu-wrap ul li a:hover{background:#02628c}
.fbpage .center-menu-wrap .centerbg {width:13px;margin:0 auto;}
.fbpage .center-menu-wrap .centerbg img {margin-bottom:-28px;}
.fbpage .professional-wrap .down-section {margin:45px auto 54px;padding:0;width:920px;height:96px;}
.fbpage .professional-wrap .down-section div.bgnone {background:none;padding:0;margin-top:-20px;width:186px;text-align:center;}
.fbpage .professional-wrap .down-section div {float:left;background:#fff;padding:26px 15px 28px;position:relative;-webkit-border-radius:3px;-moz-border-radius:3px;border-radius:3px;}
.fbpage .professional-wrap .down-section div a {background:#fe4d00;padding:6px 25px;-webkit-border-radius:3px;-moz-border-radius:3px;border-radius:3px;font-weight:600;}
.fbpage .professional-wrap .down-section div a:hover {background:#ed4800;}
.fbpage .professional-wrap .down-section div span.arrows {display:inline-block;background:url(../images/arrows.gif) no-repeat left top;width:17px;height:36px;position:absolute}
.fbpage .professional-wrap .down-section div .right {right:-16px;top:21px}
.fbpage .professional-wrap .down-section div .left {left:-16px;top:21px;background:url(../images/arrows.gif) no-repeat right top !important;}
.footer {background:#262626;text-align:center;font:400 13px/18px 'Open Sans',Helvetica,Arial,sans-serif;padding:10px 0;color:#fff;clear:both;}
.footer a {text-decoration:underline;color:#fff;}
.footer a:hover {text-decoration:none;}
span.separter {background:url(../images/seprater.gif) no-repeat top right;padding-right:5px;margin-right:8px;}

/* Popup CSS Document */
#cboxTitle {display:none !important;}
#colorbox {top:50px !important;}
#invite_friends {padding:14px 54px !important;height:380px !important;}
#invite_friends .popup-header h2 {color:#189487;display:inline-block;margin:0;padding:0 10px 0 0;font:600 23px/25px 'Open Sans',Helvetica,Arial,sans-serif;}
#invite_friends .popup-header input {display:inline-block;}

#invite_friends .popup-content ul.pic-slider {margin:15px 0;padding:20px 0; border-top:1px solid #e5e5e5;border-bottom:1px solid #e5e5e5;height:224px;}
#invite_friends .popup-content ul.pic-slider li {list-style:none;display:inline-block;width:24%;font:400 15px/19px 'Open Sans',Helvetica,Arial,sans-serif;padding-bottom:5px}
#invite_friends .popup-content ul.pic-slider li span.profile_pic{float:left;margin:0 8px;}
#invite_friends .popup-content ul.pic-slider li span.profile_pic img {border:1px solid #d9d9d9;}
#invite_friends .popup-content ul.pic-slider li input, #invite_friends .popup-content ul.pic-slider li label {vertical-align:top;float:left;margin-top:8px;}
#invite_friends .popup-content ul.pic-slider li input {margin-top:12px;}
#invite_friends .popup-footer {text-align:center;margin-top:41px;}
#invite_friends .popup-footer a {font:600 18px/30px 'Open Sans',Helvetica,Arial,sans-serif;padding:10px 19px}
.org {background:#ee6d35;}
.org:hover {background:#d6541c;}
.as-nav {display:none;}

.inline_small .popup-header h2 {color:#189487;display:inline-block;margin:0;padding:0 10px 0 0;font:600 18px/25px 'Open Sans',Helvetica,Arial,sans-serif;}
.inline_small  .popup-content {padding:20px 0;}
.inline_small  .popup-content input {padding:10px;border:1px solid #d9d9d9;width:297px;margin:0 auto;font:400 16px/20px 'Open Sans',Helvetica,Arial,sans-serif;color:#444}
.inline_small  .popup-footer {text-align:center;margin-top:12px}
.confirm-btn {padding:11px 19px;}
/*Browser CSS*/
.safari .fbpage .rescue-text h2 {font:600 25px/32px 'Open Sans',Helvetica,Arial,sans-serif;margin:32px 0 5px;padding:0}
.safari .fbpage .box p.title {font:600 23px/25px 'Open Sans',Helvetica,Arial,sans-serif;padding-bottom:10px;}
.safari .fbpage .box .view-animal {background:#fe4d00;display:inline-block;color:#fff;padding:0 20px;font:600 16px/36px 'Open Sans',Helvetica,Arial,sans-serif;-webkit-border-radius:3px;-moz-border-radius:3px;border-radius:3px;}

.ie8 .fbpage .top-menu-wrap {width:630px;}
.ie8 .fbpage .professional-wrap .down-section div.bgnone {width:170px}
.ie8 .fbpage {margin:0 auto;padding:0;max-width:997px;}
.ie8 .fbpage .center-section .donate {padding-left:10px;padding-right:0;}
.ie8 .fbpage .center-section .rescue {padding-right:10px;}
.ie8 .fbpage .box .view-animal {font:700 14px/36px "Open Sans",Helvetica,Arial,sans-serif}
.ie8 .fbpage .box p.title {font:700 20px/25px "Open Sans",Helvetica,Arial,sans-serif}
.ie8 .fbpage .box {width:230px;}
.ie8 .fbpage .center-section .rescue {width:480px;}
.ie8 .fbpage .center-section .donate {width:482px;}
.ie8 .fbpage .professional-wrap .down-section {width:952px}

.ie .inner-head-right {width:570px;}
.ie .header .fblink-wrap {padding-right:10px;}
.ie .inner-head-right .invite-btn-wrap {float:left;padding-bottom:10px;margin-left:10px;}
.ie .inner-head-right .search-wrap {float:right;border:none;border-top:1px solid #d6cece;padding:10px 0;}
.ie .search-wrap select{margin-left:0;}
.ie .fb-fundraiser-wrap ul li {width:186px;}
.ie .fb-fundraiser-wrap ul li img {width:100%;}
.ie .search-wrap input.serch-inp {padding-top:6px;padding-bottom:6px;}

/*Facebook Fundraiser Page*/
.fullpage {max-width:1128px;padding:0 10px;margin:0 auto}
.inner-head-right {width:62%;float:right;}
.inner-head-right .invite-btn-wrap {float:left;}
.inner-head-right .search-wrap {float:left;width:auto;padding:0 15px;margin:0 15px 0 0;border-left:1px solid #d6cece;border-right:1px solid #d6cece;}
.search-wrap select, .search-wrap input.serch-inp {border:1px solid #dedede;font:400 14px/18px "Open Sans",Helvetica,Arial,sans-serif;padding:6px 10px;margin-left:10px;;float:left;}
 .search-wrap input.serch-inp {padding:0 10px;font:400 14px/34px "Open Sans",Helvetica,Arial,sans-serif;}
.search-wrap input.search-btn {padding:8px 10px;margin-left:10px;}
.search-wrap form {float:left;}
.inner-head-right .search-wrap a {color:#ee304d;text-decoration:underline;font:400 13px/18px "Open Sans",Helvetica,Arial,sans-serif;float:left;padding:5px 5px 0 10px}
.search-wrap .fa-angle-right {color:#ee304d;padding-top:7px;}
.inner-head-right .search-wrap a:hover {text-decoration:none;}
.search-wrap input.search-btn {background:#ee304d;color:#fff;font:600 14px/34px "Open Sans",Helvetica,Arial,sans-serif;border:none;float:left;margin:0;padding:0 10px}
.search-wrap input.search-btn:hover {background:#c90f2b;}
.fb-page-top {float:left;width:100%;background:#fcfcfc;border-bottom:1px solid #e3e3e3;border-top:1px solid #0891c2;padding:0;}
.fb-page-top h2.title {color:#444;font:600 21px/45px "Open Sans",Helvetica,Arial,sans-serif;padding-left:22px;display:inline-block;}
.breadcrumb-wrap {float:right;width:80%;padding-right:10px;}
.breadcrumb {float: right;font: 14px "Open Sans",Helvetica,Arial,sans-serif;margin: 0;padding: 11px 10px;}
.breadcrumb li {color: #6c6c6c;display:inline-block;}
.breadcrumb li a {color: #c51f53;text-decoration: none;}
.breadcrumb li i {display: inline-block;text-align: center;width: 10px;}

.pagination-wrap {float:left;width:100%;margin:24px auto 14px;}
.pagination-wrap p.entries {font: 14px "Open Sans",Helvetica,Arial,sans-serif;float:left;padding-top:5px;}
.pagination-wrap .pagination {float:right;}
.pagination-wrap .pagination ul#pagination-list {border:1px solid #dddddd;display:inline-block;}
.pagination-wrap .pagination ul#pagination-list li {float:left;list-style:none;}
.pagination-wrap .pagination ul#pagination-list li a {float:left;padding:5px 10px;border-right:1px solid #ddd;color:#428bca;}
.pagination-wrap .pagination ul#pagination-list li.first a {padding:5px 15px;color:#999db3;}
.pagination-wrap .pagination ul#pagination-list li.last a {border-right:0;padding:5px 15px;color:#999db3;}
.pagination-wrap .pagination ul#pagination-list li a:hover, .pagination-wrap .pagination ul#pagination-list li a.active {background:#eeeeee;color:#51518b;}
.fb-fundraiser-wrap {width:100%;margin:0 auto;}
.fb-fundraiser-wrap ul li {float:left;width:270px;list-style:none;padding:0;border:1px solid #dddddd;margin:0px 10px 20px 0;font: 400 14px "Open Sans",Helvetica,Arial,sans-serif;} 
.fb-fundraiser-wrap ul li.last {margin-right:0;} .fb-fundraiser-wrap ul li:last-child {margin-right:0 !important;}
.fb-fundraiser-wrap ul li .animal-des {border-bottom:1px solid #f7d0d4;padding-bottom:10px;margin-bottom:10px;height:80px;}
.fb-fundraiser-wrap ul li .animal-des span {display:block;font: 600 18px/22px "Open Sans",Helvetica,Arial,sans-serif;color:#444;padding-bottom:10px;word-break:break-all;}
.fb-fundraiser-wrap ul li .animal-raised div {width:50%;float:left;font: 400 18px/20px "Open Sans",Helvetica,Arial,sans-serif;margin-bottom:10px;}
.fb-fundraiser-wrap ul li .animal-raised div span {color:#ee6d35;display:block;}
.fb-fundraiser-wrap ul li .box-content{padding:10px 10px 0;}
.fb-fundraiser-wrap ul li .donate-now-btn {float:left;width:100%;}
.fb-fundraiser-wrap ul li .donate-now-btn a {background:#ee304d;color:#fff;font: 400 19px/46px "Open Sans",Helvetica,Arial,sans-serif;text-align:center;display:block;}
.fb-fundraiser-wrap ul li .donate-now-btn a:hover {background:#db233f;}
.mart0 {margin-top:0;}
/*.fb-fundraiser-wrap ul li .animal-img {height:160px;}*/
.donation-page-content {max-width:1135px;margin:32px auto 0;}
.donation-page-content .profile-img {width:315px;margin-right:25px;float:left}
.donation-page-content .donation-info {width:657px;float:left}
@media (max-width:1135px){
	.fbpage {margin:0 auto;padding:0;max-width:997px;}
	.fbpage .center-section .donate {padding-left:10px;padding-right:0;}
	.fbpage .center-section .rescue {padding-right:10px;}
	.fbpage .box .view-animal {font:700 14px/36px "Open Sans",Helvetica,Arial,sans-serif}
	.fbpage .box p.title {font:700 20px/25px "Open Sans",Helvetica,Arial,sans-serif}
	.fbpage .box {width:230px;}
	.fbpage .center-section .rescue {width:480px;}
	.fbpage .center-section .donate {width:482px;}
	
	.inner-head-right {width:58%;}
	.header .fblink-wrap {padding-right:10px;}
	.inner-head-right .invite-btn-wrap {float:left;padding-bottom:10px;margin-left:10px;}
	.inner-head-right .search-wrap {float:right;border:none;border-top:1px solid #d6cece;padding:10px 0;}
	.search-wrap select{margin-left:0;}
	.fb-fundraiser-wrap ul li {width:187px;}
	.fb-fundraiser-wrap ul li img {width:100%;}
	
	.fullpage {max-width:997px;}
}

@media screen and (max-width:1135px\0) {
/* IE9 , IE10 ,IE11 rule sets go here */
	.fbpage {margin:0 auto;padding:0;max-width:997px;}
	.fbpage .center-section .donate {padding-left:10px;padding-right:0;}
	.fbpage .center-section .rescue {padding-right:10px;}
	.fbpage .box .view-animal {font:700 14px/36px "Open Sans",Helvetica,Arial,sans-serif}
	.fbpage .box p.title {font:700 20px/25px "Open Sans",Helvetica,Arial,sans-serif}
	.fbpage .box {width:230px;}
	.fbpage .center-section .rescue {width:480px;}
	.fbpage .center-section .donate {width:482px;}	
}
