.tab-marks {
    display: block !important;
    position: absolute;
    z-index: 0;
    top: -9999px;
    width: 100%;
}
.tab-marks.active {
    position: relative;
    top: 0;
}
#mapsvg, #mapsvg-viewbox-edit {
    position: relative;
    top: 0;
}
#mapsvg.hidden, #mapsvg-viewbox-edit.hidden {
    position: absolute;
    top: -9999px;
}
