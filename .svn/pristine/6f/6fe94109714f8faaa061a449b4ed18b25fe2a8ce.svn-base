<?php $base_url = $this->config->item('base_url');
$display_all_data = 'no';
$first =	'no';
$logged_in_userid = ($this->session->userdata('userid')) ? $this->session->userdata('userid') : '';
$transport_id = ($this->session->userdata('transport_id')) ? $this->session->userdata('transport_id') : '';



if($transport_id > 0)
{
	$transport_co_data = $this->common_function->get_transport_coordinator($transport_id);
	$transport_co_id = $transport_co_data['in_trans_coods_id'];
	if($transport_co_id == $logged_in_userid)
	{
	$display_all_data = 'yes';
	}
	
	$org_id = $this->common_function->get_transport_organization($transport_id);
	if($logged_in_userid > 0 && isset($org_id) && !empty($org_id) && isset($org_id['in_organization_id'])) 
	{
		$owner_id = $this->common_function->get_org_owner_id($logged_in_userid, $org_id['in_organization_id']);
		$permission = $this->common_function->get_permission($logged_in_userid, $org_id['in_organization_id']);
		//echo "<pre>";print_r($permission);die;
		if($owner_id == $logged_in_userid)
		{
			$display_all_data = 'yes';
		}
		elseif(isset($permission) && !empty($permission))
		{
			$permission_details = unserialize($permission);
			if(isset($permission_details['transport']) && (in_array('transport_cr',$permission_details['transport']) || in_array('transport_ed',$permission_details['transport']) || in_array('transport_cnf',$permission_details['transport']) || in_array('transport_del',$permission_details['transport'])))
			{
				$display_all_data = 'yes';
			}
		}
	}
}
