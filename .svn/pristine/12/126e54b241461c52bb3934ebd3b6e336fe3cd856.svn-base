class Add_fundraiser extends CI_Controller {

	public function __construct() {
		parent::__construct();
		$this->output->enable_profiler(FALSE);
		$this->load->model('common_model');
		$this->load->model('add_fundraiser_model');
		$this->load->library('form_validation');
		$this->load->helper('url');
		$this->load->library('common_function');
		$this->load->helper('cookie');
		if($this->router->method != 'campaign_page' && $this->session->userdata('org_id') == false) {
			redirect(base_url().'my-profile');
		}

	}	
	
	#=========================================================
	#	Function : sponsorship_select_campaign
	#	Purpose  : Create Sponsorship campaign
	#=========================================================
	
	public function sponsorship_select_campaign($fundraiser_id='') {
		if(!$this->session->userdata('userid')) {
			redirect($this->config->item('base_url'));
		}
		//echo $this->session->userdata('last_campaign_id');die;
		($this->session->userdata('last_campaign_id')==true)?$this->session->unset_userdata('last_campaign_id'):'';
		($this->session->userdata('edit_campaign_id')==true)?$this->session->unset_userdata('edit_campaign_id'):'';
		($this->session->userdata('st_custom_url')==true)?$this->session->unset_userdata('st_custom_url'):'';
		
		$data = array();
		$data['menu_no'] 		= 'yes';
		if($fundraiser_id != '') {
			$data['fundraiser_id'] = $fundraiser_id;
			$data['check_fundraiser_id']	=	$this->add_fundraiser_model->check_fundraiser_id($fundraiser_id);
			
			if($data['check_fundraiser_id'] == 0) {
				redirect(base_url().'sponsorship/sponsorship_list');
			}
		}
		
		//$this->session->set_userdata('fundraiser_id',$fundraiser_id);
		//$data['animal_name'] = $this->add_fundraiser_model->get_animal_details($animal_id);
		
		$this->load->view('add_fundraiser/sponsorship-select-campaign',$data);
	}
	
	
