function get_checked_count() {
	var general_count = $('input[name="general[]"]:checked:visible').length;
	var medical_count = $('input[name="medical[]"]:checked:visible').length;
	var transport_count = $('input[name="transport[]"]:checked:visible').length;
	var foster_home_count = $('input[name="foster_home[]"]:checked:visible').length;
	var total_checked_count = Number(general_count)+Number(medical_count)+Number(transport_count)+Number(foster_home_count);
	return total_checked_count;
}

