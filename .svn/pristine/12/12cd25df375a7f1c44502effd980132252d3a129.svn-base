$('.rabiesvac').click(function(){
	var get_id = $(this).attr('id');
	if(get_id == "rabiesvac-yes") {
		if($('#'+get_id).is(':checked')) {
			$('#rabiesvac_date').closest('.marb10').show();
		} else {
			$('#rabiesvac_date').closest('.marb10').hide();
		}
	} else if(get_id == "rabiesvac-no") {
			$('#rabiesvac_date').closest('.marb10').hide();
			
			$('#rabiesvac-unknown').closest('span').removeClass('checked');
			$('#rabiesvac-yes').closest('span').removeClass('checked');
			$('#rabiesvac-no').closest('span').addClass('checked');
	} else {
			$('#rabiesvac_date').closest('.marb10').hide();
			
			$('#rabiesvac-yes').closest('span').removeClass('checked');
			$('#rabiesvac-no').closest('span').removeClass('checked');
			$('#rabiesvac-unknown').closest('span').addClass('checked');
	}
});

