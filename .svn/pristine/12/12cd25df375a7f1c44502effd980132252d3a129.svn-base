$(window).on("load", function (){
	var get_animal_type = $('#animal-type').val();
	
	if(get_animal_type == 4) {
		$('#declawed_setting').closest('label').hide();
		$('#trained_setting').closest('label').show();
		
		/*$('#other_setting').closest('label').html('<input type="checkbox" name="foster_home[]" id="other_setting" value="other">Good with other dogs?');
			
		$('#same_setting').closest('label').html('<input type="checkbox" name="foster_home[]" id="same_setting" value="same">Good with cats?');
		App.init();*/
		
		$('.good-dogs').html('Good with other dogs?');
		$('.good-cats').html('Good with cats?');
		
		
	} else  if(get_animal_type == 3) {
		$('#declawed_setting').closest('label').show();
		$('#trained_setting').closest('label').hide();
		
		/*$('#other_setting').closest('label').html('<input type="checkbox" name="foster_home[]" id="other_setting" value="other">Good with other cats?');
			
		$('#same_setting').closest('label').html('<input type="checkbox" name="foster_home[]" id="same_setting" value="same">Good with dogs?');
		App.init();*/
		
		$('.good-dogs').html('Good with other cats?');
		$('.good-cats').html('Good with dogs?');
	}
	
	if(get_animal_type == 2) {
		$('#birdsex_setting').closest('label').show();
	}
	
	if($('#rabiesvac-yes').is(':checked')) {
		$('#rabiesvac_date').closest('.marb10').show();
	}
	if($('#fecaltest-yes').is(':checked')) {
		$('#fecaltest_date').closest('.marb10').show();
	}
	if($('#microchipp-yes').is(':checked')) {
		$('#chipped_animals_id').closest('.marb10').show();
	}
	if($('#heartworm-yes').is(':checked')) {
		$('#heartworm_date').closest('.marb10').show();
		$('#heartworm_vaccine_brand').show();
	}
	if($('#bordatella-yes').is(':checked')) {
		$('#bordatella_date').closest('.marb10').show();
		$('#bordatella_vaccine_brand').show();
	}
	if($('#surgery-yes').is(':checked')) {
		$('#surgery_date').closest('.marb10').show();
	}
	if($('#quarantineyes').is(':checked')) {
		$('#quarantine_end_date').closest('.marb10').show();
	}
	if($('.weightunknown').is(':checked')) {
		$('#weight').attr('disabled',true);
		$('#weight').val('');
	}
	var profile_visibility = $('.profile_visible:checked').val();
	
	if(profile_visibility == 1) {
		$('input[name="general[]"]').attr('disabled',true);
		$('input[name="medical[]"]').attr('disabled',true);
		$('input[name="transport[]"]').attr('disabled',true);
		$('input[name="foster_home[]"]').attr('disabled',true);
		$('#check_all').attr('disabled',true);
	}
	
	/*if(get_animal_type == 2) {
		var total_count = get_checked_count();
		$('#check_all').closest('span').addClass('checked');
		//$('#check_all').attr('checked',true);
	}*/
});

