body.orange {background:#fff url(../../img/orange/pagebg.gif) repeat-x !important;}
body.orange img {max-width:100%;}
body.orange #wrap {max-width:997px; margin:0 auto;background:#fff;}
.page-header-fixed .page-container.mart0 {margin-top:0;}


body.orange .progress > .progress-bar-info{background-color: #e88c39;}
body.orange .donation-form{border: 1px solid #e9dbcf;}
body.orange .donation-form h2{ color:#763901;}
body.orange .donation-form .form-content .control-label.special{color:#763901;}
body.orange .donation-form .form-content .controls.amount .radio{ color:#763901;}
body.orange .donation-form .form-content .controls input{background: #f2f2f2; /* Old browsers */
/* IE9 SVG, needs conditional override of 'filter' to 'none' */
background: url(data:image/svg+xml;base64,) !important;
background: -moz-linear-gradient(top,  #f2f2f2 0%, #ffffff 14%, #ffffff 100%) !important; /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f2f2f2), color-stop(14%,#ffffff), color-stop(100%,#ffffff)) !important; /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  #f2f2f2 0%,#ffffff 14%,#ffffff 100%) !important; /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  #f2f2f2 0%,#ffffff 14%,#ffffff 100%) !important; /* Opera 11.10+ */
background: -ms-linear-gradient(top,  #f2f2f2 0%,#ffffff 14%,#ffffff 100%) !important; /* IE10+ */
background: linear-gradient(to bottom,  #f2f2f2 0%,#ffffff 14%,#ffffff 100%) !important; /* W3C */
color:#787878 !important; font:normal 18px/22px 'Roboto',sans-serif; padding: 10px 9px !important; height:auto; border:1px solid #d2d2d2;
}/*font:normal 19px/19px 'Open Sans', Helvetica, Arial, sans-serif;*/
/* donation form step 1 - rightpart*/
body.orange .recent-donations h2{ border-bottom:1px solid #f5e6d8;}
body.orange .recent-donations .bg {background:#fff2e6; }
body.orange .recent-donations .donation-comment{border:1px solid #e9dbcf;}
body.orange .recent-donations .donation-comment p.amount{ color:#763901; }
body.orange .donation-rightpart .rightpart-box .bg .view-profile, body.orange .donation-rightpart .rightpart-box .bg .view-profile:hover, body.orange .donation-rightpart .rightpart-box .bg p span{color:#763901;}/*0A558E*/


/*Profile Title*/
body.orange .donation-detail .page-title { background:#1679C4 url(../../img/orange/pagebg.gif) repeat-x 0 -145px;}

/*progress & donation/pledge detail*/
body.orange .progress {box-shadow:0 1px 2px rgba(0, 0, 0, 0.1) inset!important; background-image:linear-gradient(to bottom, #E6E6E6, #ECECEC)!important;}
body.orange .progress .bar {background-color:#6abfff; background-size: 70px 70px;}
body.orange .raised-amount, body.orange .goal-amount {background:url(../../img/white-transperent-bg.png) repeat 0 0; color:#602f01; }

body.orange .btnDonatePledge {color:#fff; border-bottom:2px solid #6b3301; background: #c3742d; /* Old browsers */
/* IE9 SVG, needs conditional override of 'filter' to 'none' */
background: url(data:image/svg+xml;base64,);
background: -moz-linear-gradient(top,  #c3742d 0%, #964700 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#c3742d), color-stop(100%,#964700)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  #c3742d 0%,#964700 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  #c3742d 0%,#964700 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top,  #c3742d 0%,#964700 100%); /* IE10+ */
background: linear-gradient(to bottom,  #c3742d 0%,#964700 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#c3742d', endColorstr='#0b8e01',GradientType=0 ); /* IE6-8 */}  

body.orange .btnDonatePledge:hover, body.orange .btnDonatePledge:focus{color:#fff; background: #c3742d; /* Old browsers */
/* IE9 SVG, needs conditional override of 'filter' to 'none' */
background: url(data:image/svg+xml;base64,);
background: -moz-linear-gradient(top,  #964700 0%, #c3742d 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#964700), color-stop(100%,#c3742d)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  #964700 0%,#c3742d 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  #964700 0%,#c3742d 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top,  #964700 0%,#c3742d 100%); /* IE10+ */
background: linear-gradient(to bottom,  #964700 0%,#c3742d 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#964700', endColorstr='#c3742d',GradientType=0 ); /* IE6-8 */}

body.orange .time {background:#eef5ee url(../../img/icon-clock.png) no-repeat 9px center; border:3px solid #e6f0e5; color:#2a2a2a;}
body.orange .time .day-remaining {color:#2a2a2a;}
body.orange .time .text {color:#2a2a2a; padding-left: 8px;}

body.orange .donation-needed .donation-slider { background:url(../../img/orange/slider.png) repeat-x 0px 8px;}
body.orange .donation-needed .donation-slider .slider {background:url(../../img/orange/slider.png) repeat-x 0px -38px; }
body.orange .donation-needed .donation-slider .slider span {border:1px solid #585858; color:#fff; background: #b45601; /* Old browsers */
/* IE9 SVG, needs conditional override of 'filter' to 'none' */
background: url(data:image/svg+xml;base64,);
background: -moz-linear-gradient(top,  #b45601 0%, #7f3d01 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#b45601), color-stop(100%,#7f3d01)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  #b45601 0%,#7f3d01 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  #b45601 0%,#7f3d01 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top,  #b45601 0%,#7f3d01 100%); /* IE10+ */
background: linear-gradient(to bottom,  #b45601 0%,#7f3d01 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#b45601', endColorstr='#7f3d01',GradientType=0 ); /* IE6-8 */}

body.orange .donation-needed .total {background:#f5f5f5; color:#b45601;}

body.orange .social-icons {margin:18px 0 10px;}
body.orange .social-icons img {margin:0 5px 10px 0;}

body.orange .donation-pledge-detail .article-block .main-article {border:1px solid #e6cab0;}
body.orange .donation-pledge-detail .article-block .main-article .bg {background:#f8f8f8; }
body.orange .donation-pledge-detail .article-block .main-article .bg p span {color:#763901;}
body.orange .donation-pledge-detail .article-block .main-article input.fb-link { border:1px solid #e2e2e2; background:#fff; color:#4b7276;}
body.orange .donation-pledge-detail .article-block .main-article input.fb-link:focus{border-color: #999999;}
body.orange .donation-pledge-detail .article-block .main-article .btnShare {background:url(../../img/btn-share.gif) no-repeat 0 0;}
body.orange .view-profile {color:#763901; background: url(../../img/orange/arrow-right.gif) no-repeat right 8px; }
body.orange .view-profile:hover {color:#763901; }
body.orange .donation-pledge-detail h1 {color:#763901;}
body.orange .donation-pledge-detail .media .posted-by{ color:#8d8c8c;}
body.orange .donation-pledge-detail .media .posted-by span{color:#763901;}
body.orange .donation-pledge-detail .media .media-comment{ border:1px solid #e8e8e8;}
body.orange .donation-pledge-detail .media .media-heading{color:#763901; background:url(../../img/comment-bubble-top.gif) no-repeat 0px 0px; }

