@charset "utf-8";
/* CSS Document */
body {background:url(images/powbg.jpg); margin:0; padding:0; font:400 14px "Open Sans",Arial, Helvetica, sans-serif; color:#535353; text-size-adjust:none; -moz-text-size-adjust:none; -webkit-text-size-adjust:none; -ms-text-size-adjust:none;}

h1, h2, h3, h4, h5, h6 {font-family:"Open Sans",Arial, Helvetica, sans-serif; color:#444; line-height:1.35; text-size-adjust:none; -moz-text-size-adjust:none; -webkit-text-size-adjust:none; -ms-text-size-adjust:none;}
button{-moz-appearance:none;-ms-appearance:none;-o-appearance:none;appearance:none;-webkit-border-radius:0;}
a:link {color:#464646; -moz-transition:color 0.3s linear 0s; -webkit-transition:color 0.3s linear 0s; transition:color 0.3s linear 0s; text-decoration:none;}
a:visited, a:active {color:#464646;} 
a:hover {color:#c51e52;}
:focus, a {outline:0;}
p {text-size-adjust:none; -moz-text-size-adjust:none; -webkit-text-size-adjust:none; -ms-text-size-adjust:none;}

#container {width:1240px; margin:0 auto;}
#main {overflow:hidden;}

.br {clear:both; margin:0; padding:0;}
img {border:none; outline:0;}

/*Header*/
.headertop {background-color:#f4f4f4;}

#logo-home {margin:20px 0; float:left; height:77px;}

#home-nav {float:right; margin:46px 0 0;}
#home-nav ul, #home-nav li {list-style-type:none; margin:0; padding:0;}
#home-nav li {float:left; margin-left:40px;}
#home-nav li.first {margin-left:0;}
#home-nav li i {font-size:20px; color:#4d4d4d;}
#home-nav li i:hover {font-size:20px; color:#ed6e34;}
#home-nav li a {color:#4d4d4d; font:700 15px "Open Sans", Arial, Helvetica, sans-serif; text-decoration:none; text-transform:uppercase;}
#home-nav li a:hover {color:#ed6e34; font:700 15px "Open Sans", Arial, Helvetica, sans-serif; text-decoration:none;}
#home-nav li a.transport {color:#fff; font:700 15px "Open Sans", Arial, Helvetica, sans-serif; text-decoration:none; text-shadow:0 1px 0 #be582a; padding:7px 30px 9px 30px; background-color:#ed6e34;}
#home-nav li a:hover.transport {color:#fff; font:700 15px "Open Sans", Arial, Helvetica, sans-serif; text-decoration:none; background-color:#e1571b;}
/*Header*/

/*Content Area*/
#container-home {width:1240px; margin:0 auto;}
#container-home a.rescue-transport {display:none !important;}
#container-home h1 {color:#0f0f0f; font:300 40px "Open Sans", Arial, Helvetica, sans-serif; text-align:center; text-transform:uppercase;  padding:36px 0 33px 0;}
#container-home h1 span {font-weight:700; color:#5b483e;}
#container-home h1 span.d {color:#ee6d35;}
#container-home h1 span.og {color:#4b7276;}
#container-home h1 span.op {color:#c41e52;}
#container-home p.intro {color:#0f0f0f; font:400 16px "Open Sans", Arial, Helvetica, sans-serif; text-align:center; padding-bottom:7px;}
#container-home p.intro strong {font-weight:700;}
#container-home h2 {color:#373737; font:700 55px "Open Sans", Arial, Helvetica, sans-serif; text-align:center; text-transform:uppercase;  padding:27px 0 34px 0;}

#container-home #blocks4 ul, #container-home #blocks4 li {list-style-type:none; margin:0; padding:0;}

#container-home #blocks4 li {float:left; width:23.78%; margin:0 20px 30px 0; -webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;}
#container-home #blocks4 li.last {margin-right:0;}

#container-home #blocks4 li a.volunteer, #container-home #blocks4 li a:hover.volunteer {background:#ed6e34; text-decoration:none; cursor:pointer; display:block;}
#container-home #blocks4 li a.volunteer span.image {float:left; padding:14px 18px 12px 11px; height:76px;}
#container-home #blocks4 li a.volunteer span.note {float:left; color:#fff; padding:18px 0 10px 0; font:600 18px/22px "Open Sans", Arial, Helvetica, sans-serif; width:60%;}
#container-home #blocks4 li a.volunteer span.learnmore {background:#76371a; clear:both; display:block; font:600 20px "Open Sans", Arial, Helvetica, sans-serif; text-align:center; color:#fff; padding:9px 0 11px 0; text-transform:uppercase;}
#container-home #blocks4 li a.volunteer span.learnmore img {vertical-align:middle; margin:-4px 0 0 12px;}

#container-home #blocks4 li a.rescue, #container-home #blocks4 li a:hover.rescue {background:#4b7276; text-decoration:none; cursor:pointer; display:block;}
#container-home #blocks4 li a.rescue span.image {float:left; padding:23px 18px 17px 17px; height:62px;}
#container-home #blocks4 li a.rescue span.note {float:left; color:#fff; padding:18px 0 10px 0; font:600 18px/22px "Open Sans", Arial, Helvetica, sans-serif; width:60%; text-align:left;}
#container-home #blocks4 li a.rescue span.learnmore {background:#25393b; clear:both; display:block; font:600 20px "Open Sans", Arial, Helvetica, sans-serif; text-align:center; color:#fff; padding:9px 0 11px 0; text-transform:uppercase;}
#container-home #blocks4 li a.rescue span.learnmore img {vertical-align:middle; margin:-4px 0 0 12px;}

#container-home #blocks4 li a.animal, #container-home #blocks4 li a:hover.animal {background:#ee304e; text-decoration:none; cursor:pointer; display:block;}
#container-home #blocks4 li a.animal span.image {float:left; padding:24px 17px 18px 14px; height:60px;}
#container-home #blocks4 li a.animal span.note {float:left; color:#fff; padding:18px 0 10px 0; font:600 18px/22px "Open Sans", Arial, Helvetica, sans-serif; width:58%;}
#container-home #blocks4 li a.animal span.learnmore {background:#771827; clear:both; display:block; font:600 20px "Open Sans", Arial, Helvetica, sans-serif; text-align:center; color:#fff; padding:9px 0 11px 0; text-transform:uppercase;}
#container-home #blocks4 li a.animal span.learnmore img {vertical-align:middle; margin:-4px 0 0 12px;}

#container-home #blocks4 li a.shelter, #container-home #blocks4 li a:hover.shelter {background:#a5a030; text-decoration:none; cursor:pointer; display:block;}
#container-home #blocks4 li a.shelter span.image {float:left; padding:20px 22px 18px 25px; height:64px;}
#container-home #blocks4 li a.shelter span.note {float:left; color:#fff; padding:29px 0 10px 0; font:600 18px/22px "Open Sans", Arial, Helvetica, sans-serif; width:60%;}
#container-home #blocks4 li a.shelter span.learnmore {background:#525018; clear:both; display:block; font:600 20px "Open Sans", Arial, Helvetica, sans-serif; text-align:center; color:#fff; padding:9px 0 11px 0; text-transform:uppercase;}
#container-home #blocks4 li a.shelter span.learnmore img {vertical-align:middle; margin:-4px 0 0 12px;}

#container-home #blocks4 li .mobilelearn {display:none;}

#container-home span.remember {margin:0 auto 50px auto; border:1px solid #c4c4c4; text-align:center; padding-left:20px; -webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box; text-transform:uppercase; font:400 14px/27px "Open Sans", Arial, Helvetica, sans-serif; color:#535353; text-decoration:none; clear:both; display:inline-block;}
#container-home span.remember .checkbox {background:url(images/checkbox.png) no-repeat; clear:left; display:block; float:left; height:27px; padding:0; width:27px; margin-top:11px;}
#container-home span.remember label {cursor:pointer; padding:10px 20px 10px 10px; float:left;}
/*#container-home span:hover.remember {font:400 14px "Open Sans", Arial, Helvetica, sans-serif; color:#535353; text-decoration:underline;}*/
/*Content Area*/

/*Footer*/
#home-footer {background:#333333; padding-top:45px; color:#fff;}
#home-footer #footer {width:1240px; margin:0 auto;}
#home-footer .block {float:left; width:48.9%; margin:0 25px 50px 0; -webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;}
#home-footer .block.last {margin-right:0;}
#home-footer h2 {font:700 38px "Open Sans", Arial, Helvetica, sans-serif; color:#fff; padding-bottom:25px; text-transform:uppercase;}
#home-footer p {font:400 14px/26px "Open Sans", Arial, Helvetica, sans-serif; color:#a7a7a7; padding-bottom:36px;}
#home-footer p.padb0 {padding-bottom:0;}
#home-footer p span {color:#6e6e6e;}
#home-footer p.user {color:#fff;}
#home-footer p.user span {color:#ee304e;}
#home-footer a.readmore {font:400 14px/46px "Open Sans", Arial, Helvetica, sans-serif; color:#fff; padding:0 24px; text-decoration:none; text-transform:uppercase; display:inline-block; background:#ee6d35;}
#home-footer a:hover.readmore {font:400 14px/46px "Open Sans", Arial, Helvetica, sans-serif; color:#fff; text-decoration:none; background:#e1571b;}
#home-footer-social {background:#fff; padding:30px 0;}
#home-footer-social .copyright-social {width:1240px; margin:0 auto; text-align:center; font:400 14px "Open Sans", Arial, Helvetica, sans-serif; color:#707070;}
#home-footer-social .copyright-social img {vertical-align:middle; margin-right:9px;}
#home-footer-social .copyright-social img.fb {margin-left:11px;}
/*Footer*/

button.float-help {border:0; position:fixed; bottom:0; right:0; z-index:1; background:#ee6d35; color:#fff; text-decoration:none; padding:9px 21px 10px;  margin-bottom:12px; margin-right:12px; -webkit-border-radius:50px !important; -moz-border-radius:50px !important; border-radius:50px !important; font-weight: 600; font-size:15px; cursor:pointer;}
button.float-help:hover {color: #fff; background:#d55923;}

@media (max-width:1024px){
#container, #container-home, #home-footer #footer, #home-footer-social .copyright-social {width:100%; -webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box; padding:0 16px;}
#home-nav li a.transport {padding-left:18px; padding-right:18px;}
#home-nav li {margin-left:18px;}
#container-home #blocks4 li {width:23.46%;}
#container-home #blocks4 li a.volunteer span.image, #container-home #blocks4 li a.rescue span.image, #container-home #blocks4 li a.animal span.image, #container-home #blocks4 li a.shelter span.image {padding-right:13px;}
#container-home #blocks4 li a.volunteer span.note {width:56%; font-size:14px;}
#container-home #blocks4 li a.rescue span.note {width:52%; font-size:14px;}
#container-home #blocks4 li a.animal span.note {width:47%; font-size:14px;}
#container-home #blocks4 li a.shelter span.note {width:54%; font-size:14px;}
#home-footer .block {width:48.7%;}
}

@media (max-width:996px){
#container-home #blocks4 li {width:48.6%;}
#container-home #blocks4 li.two {margin-right:0;}
#container-home #blocks4 li a.volunteer span.image, #container-home #blocks4 li a.rescue span.image, #container-home #blocks4 li a.animal span.image, #container-home #blocks4 li a.shelter span.image {padding-right:18px;}
#container-home #blocks4 li a.volunteer span.note, #container-home #blocks4 li a.rescue span.note, #container-home #blocks4 li a.animal span.note,
#container-home #blocks4 li a.shelter span.note {width:60%; font-size:18px; line-height:27px;}
#home-footer .block {width:48.6%;}
}

@media (max-width:955px){
#responsive-menu-button{right:8% !important;background-color:transparent !important;position:absolute !important;}
.responsive-menu-button-icon{display:inline-block !important;}
.responsive-menu-box{width:47px !important;height:45px !important;display:inline-block !important;}
.responsive-menu-button .responsive-menu-button-icon-inactive{display:none !important;}
#home-nav {display:none;}
#container-home a.rescue-transport {color:#fff; font:700 22px "Open Sans", Arial, Helvetica, sans-serif; text-decoration:none; text-shadow:0 1px 0 #be582a; padding:14px 22px; background:url(images/rescue-arrow.png) no-repeat 98% 13px #ed6e34; display:block !important; text-transform:uppercase; margin-top:16px;}
#container-home a:hover.rescue-transport {color:#fff; font:700 22px "Open Sans", Arial, Helvetica, sans-serif; text-decoration:none; background:url(images/rescue-arrow.png) no-repeat 98% 13px #e1571b;}
}

@media (max-width:800px){
#home-footer .block {width:48.3%;}
}

@media (max-width:667px){
.responsive-menu-button {top:8% !important;}
#container-home #blocks4 li {width:48.4%;}
#container-home #blocks4 li a.volunteer span.note, #container-home #blocks4 li a.rescue span.note, #container-home #blocks4 li a.animal span.note, #container-home #blocks4 li a.shelter span.note {line-height:24px;}
#container-home #blocks4 li a.animal span.note {width:54%;}
#home-footer .block {width:48%;}
#home-footer h2 {font-size:28px; padding-bottom:18px;}
#home-footer p {line-height:21px;}
#container-home h2 {font-size:45px;}
}

@media (max-width: 667px) and (orientation: landscape) {
#container-home p.intro {font-size:12px !important;}
#home-footer-social .copyright-social {font-size:13px !important;}
}

@media (max-width:640px){
.responsive-menu-button {top:9% !important;}
#container-home #blocks4 li {width:48.2%;}
#container-home #blocks4 li a.volunteer span.note, #container-home #blocks4 li a.rescue span.note, #container-home #blocks4 li a.shelter span.note {width:56%;}
#container-home #blocks4 li a.animal span.note {width:52%;}
#home-footer .block {width:47.5%;}
}

@media (max-width:602px){
.responsive-menu-button {top:5% !important;}
#container-home #blocks4 li a.volunteer span.note, #container-home #blocks4 li a.rescue span.note, #container-home #blocks4 li a.shelter span.note {width:55%; font-size:16px;}
#container-home #blocks4 li a.animal span.note {width:50%; font-size:16px;}
}

@media (max-width:568px){
.responsive-menu-button {top:10% !important;}
#container-home #blocks4 li {width:48%;}
#container-home #blocks4 li a.volunteer span.note, #container-home #blocks4 li a.rescue span.note, #container-home #blocks4 li a.shelter span.note {width:55%; font-size:16px;}
#container-home #blocks4 li a.animal span.note {width:50%; font-size:16px;}
}

@media (max-width:480px){
#container-home #blocks4 li {margin-right:0; width:100%; margin-bottom:10px;}

#container-home #blocks4 li a.volunteer, #container-home #blocks4 li a.volunteer:hover, #container-home #blocks4 li a.rescue, #container-home #blocks4 li a:hover.rescue, #container-home #blocks4 li a.animal, #container-home #blocks4 li a:hover.animal,#container-home #blocks4 li a.shelter, #container-home #blocks4 li a:hover.shelter {float:left; width:100%; -webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;}

#container-home #blocks4 li a.volunteer span.learnmore, #container-home #blocks4 li a.rescue span.learnmore, #container-home #blocks4 li a.animal span.learnmore,
#container-home #blocks4 li a.shelter span.learnmore {display:none;}

#container-home #blocks4 li .mobilelearn {display:inline-block; float:right; margin:20px 20px 0 0;}
#container-home #blocks4 li .mobilelearn img {max-width:100%;}
#container-home #blocks4 li a.volunteer span.note, #container-home #blocks4 li a.rescue span.note, #container-home #blocks4 li a.shelter span.note,
#container-home #blocks4 li a.animal span.note {font-size:17px; padding-top:24px;}
#container-home span.remember {margin-top:20px; /*width:100%;*/}
#home-footer .block {width:100%; margin-right:0;}
}

@media (max-width:375px){
.responsive-menu-button {top:5% !important;}
#container-home h1 {font-size:40px; line-height:46px;}
#container-home #blocks4 li .mobilelearn {width:40px; margin:31px 12px 0 0;}
#container-home #blocks4 li a.volunteer span.note, #container-home #blocks4 li a.rescue span.note, #container-home #blocks4 li a.shelter span.note {width:51%; padding-top:14px; font-size:16px;}
#container-home #blocks4 li a.animal span.note {width:45%; padding-top:14px; font-size:16px;}
#container-home #blocks4 li a.shelter span.note {padding-top:43px;}
#home-footer-social .copyright-social span {display:block; margin-top:12px;}
#container-home a.rescue-transport {font-size:18px; background:url(images/rescue-arrow-sm.png) no-repeat 95% 15px #ed6e34; padding-bottom:12px;}
#container-home a:hover.rescue-transport {font-size:18px; background:url(images/rescue-arrow-sm.png) no-repeat 95% 15px #e1571b;}
}

@media (max-width:360px){
#container-home #blocks4 li a.rescue span.note {width:47%;}
#container-home h1 {font-size:38px; line-height:41px;}
}

@media (max-width:320px){
#container-home #blocks4 li a.volunteer span.image, #container-home #blocks4 li a.rescue span.image, #container-home #blocks4 li a.animal span.image, #container-home #blocks4 li a.shelter span.image {width:54px; padding-bottom:9px;}
#container-home #blocks4 li a.volunteer span.image img, #container-home #blocks4 li a.rescue span.image img, #container-home #blocks4 li a.animal span.image img, #container-home #blocks4 li a.shelter span.image img {max-width:100%;}
#container-home #blocks4 li a.volunteer span.note, #container-home #blocks4 li a.rescue span.note {width:48%; padding-top:14px; font-size:14px; padding-bottom:0;}
#container-home #blocks4 li a.animal span.note, #container-home #blocks4 li a.shelter span.note {width:45%; padding-top:14px; font-size:14px; padding-bottom:0;}
#container-home #blocks4 li a.shelter span.note {padding-top:25px;}
#container-home #blocks4 li .mobilelearn {width:40px;}
#container-home h2 {font-size:40px; padding:23px 0 27px;}
#container-home a.rescue-transport {font-size:16px; background:url(images/home-arrow.png) no-repeat 95% 14px #ed6e34; padding-bottom:12px; padding-top:11px;}
#container-home a:hover.rescue-transport {font-size:16px; background:url(images/home-arrow.png) no-repeat 95% 14px #e1571b; padding-top:11px;}
}