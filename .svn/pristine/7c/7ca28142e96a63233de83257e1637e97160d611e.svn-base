<script type="text/javascript">
function readURL(input) {
	if (input.files && input.files[0]) {
		var reader = new FileReader();

		reader.onload = function (e) {
			if(input.id == 'photo1') {
				$('#img_wh_chk1').attr('src', e.target.result);
			} else if(input.id == 'photo2') {
				$('#img_wh_chk2').attr('src', e.target.result);
			} else if(input.id == 'photo3') {
				$('#img_wh_chk3').attr('src', e.target.result);
			} else if(input.id == 'photo4') {
				$('#img_wh_chk4').attr('src', e.target.result);
			}
		}

		reader.readAsDataURL(input.files[0]);
		
	}

}

</script>
