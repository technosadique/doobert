		$where .= "|| (`tr`.`in_trans_coods_id`= ".$this->session->userdata('userid')." ))";
		if($status == '1')
		{
			$where .= " AND ((tr.in_status = '1') || (tr.in_status = '2' && tr.dt_target >= current_date()))";
		}
		else if($status == '2')
		{
			$where .= " AND (tr.in_status = '2' && tr.dt_target < current_date())";
		}
		else
		{
			$where .= " AND tr.in_status = '".$status."'";
