<!-- Being Youtube URL popup1-->
<div class="modal fade" id="youtube-url1" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" data-backdrop="static" data-keyboard="false">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button aria-hidden="true" data-dismiss="modal" class="close close_modal1" type="button"></button>
        <h4 class="modal-title add-pop-title">Add Video</h4>
      </div>
      <div class="modal-body">
        <div class="form-group">
          <label for="organization-website">Please copy and paste your YouTube video URL below: </label>
          <div class="input-group col-lg-12 no-space">
            <input type="text" class="form-control" id="organization-website1" placeholder="http://">
            <div class="clearfix"></div>
          </div>
          <span class="help-block">(e.g. http://www.youtube.com/watch?v=qPTg49JrLu4)</span> 
          <span class="help-block url_error1 has-error-video" style="display:none">Please enter valid URL</span>
          </div>
      </div>
      <div class="modal-footer text-center no-margin">
        <button type="button" class="btn orange pull-left add-youtube-link1"><i class="fa fa-plus"></i> Add Video</button>
      </div>
    </div>
    <!-- /.modal-content --> 
  </div>
  <!-- /.modal-dialog --> 
</div>
<!-- End Youtube URL popup-->

