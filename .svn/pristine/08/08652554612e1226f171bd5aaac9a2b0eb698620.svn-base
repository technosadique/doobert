<script>
$(document).on('keyup','#fb_emailid',function(){
	
	if($('#fb_emailid').val()!='')
	{
		
		$('#submit-fbuser-email-unchecked').hide();
		$('#submit-fbuser-email').show();
	} else {
		//$('#submit-term-condition').addClass('disabled');
		$('#submit-fbuser-email-unchecked').show();
		$('#submit-fbuser-email').hide();
	}	
	
});




$(document).on('click','#submit-fbuser-email',function()
{
		if($('#fb_emailid').val()!='')
		{
			var emailReg = /^([\w-\.\+]+@([\w-]+\.)+[\w-]{2,4})?$/;
			var useremail = $("#fb_emailid").val();
			var error_message = '';			
			if(emailReg.test(useremail))
			{
				$.ajax({
						type:'POST',
						 url: "<?php echo base_url(); ?>user/update_email/",
						 dataType: "json",
						 async:false,
						 data:{
							fb_emailid:useremail,
						 },
						 success:function(data) 
						 {
							if(data==0)
							{
									alert('Email already exist, please specify other one.');
							} else {
								$('#fb_user_email').modal('hide');
								$('#fb_user_email_thankyou').modal('show');
							}
						 }
					});	
			} else 
			{				
				error_message = 'Please enter valid email address. \n';
				alert(error_message);
			}										
		} else {
			alert('Please enter email address.');
		}	
});
</script>
