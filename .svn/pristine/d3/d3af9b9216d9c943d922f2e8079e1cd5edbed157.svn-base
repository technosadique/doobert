    src: url('MuseoSlabW01-300.eot?#iefix') format('embedded-opentype'),
        url('MuseoSlabW01-300.woff2') format('woff2'),
        url('MuseoSlabW01-300.woff') format('woff'),
        url('MuseoSlabW01-300.ttf') format('truetype'),
        url('MuseoSlabW01-300.svg#MuseoSlabW01-300') format('svg');
    font-weight: 300;
