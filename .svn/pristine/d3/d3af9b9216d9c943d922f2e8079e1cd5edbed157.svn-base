@font-face {
    font-family: 'SofiaProbold';
    src: url('SofiaProSoftW01-Bold.eot');
    src: url('../asset/fonts/SofiaProSoftW01-Bold.eot?#iefix') format('embedded-opentype'),
        url('../asset/fonts/SofiaProSoftW01-Bold.woff2') format('woff2'),
        url('../asset/fonts/SofiaProSoftW01-Bold.woff') format('woff'),
		
        url('../asset/fonts/SofiaProSoftW01-Bold.ttf') format('truetype'),
        url('../asset/fonts/SofiaProSoftW01-Bold.svg#SofiaProSoftW01-Bold') format('svg');
    font-weight: bold;
    font-style: normal;
}



@font-face {
    font-family: 'MuseoSlabW01-300';
