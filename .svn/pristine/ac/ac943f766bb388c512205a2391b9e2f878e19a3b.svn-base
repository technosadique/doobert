	public function get_filtered_name($dname){
		if(!is_null($dname)){
			$dname = trim($dname);
			$dname = str_replace(' ','-',trim($dname));
			$dname = preg_replace("![^a-z0-9&~_-]+!i", "", html_entity_decode($dname));
			$dname = str_replace('---','-',trim($dname));			
			if(substr($dname,-1) == '.')
				$dname = substr($dname,0,-1);
		}
		return strtolower($dname);
	}
	
