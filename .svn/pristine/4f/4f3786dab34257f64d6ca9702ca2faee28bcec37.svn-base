body.skyblue {background:url(../../img/skyblue/pagebg.gif) repeat-x #fff !important;}
body.skyblue img {max-width:100%;}
body.skyblue #wrap {max-width:997px; margin:0 auto; background:#fff;}
.page-header-fixed .page-container.mart0 {margin-top:0;}


body.skyblue .progress > .progress-bar-info{background-color: #3dc1ec;}
body.skyblue .donation-form{border: 1px solid #cfe3e9;}
body.skyblue .donation-form h2{ color:#015e7d;}
body.skyblue .donation-form .form-content .control-label.special{color:#015e7d;}
body.skyblue .donation-form .form-content .controls.amount .radio{ color:#015e7d;}
body.skyblue .donation-form .form-content .controls input{background: #f2f2f2; /* Old browsers */
/* IE9 SVG, needs conditional override of 'filter' to 'none' */
background: url(data:image/svg+xml;base64,) !important;
background: -moz-linear-gradient(top,  #f2f2f2 0%, #ffffff 14%, #ffffff 100%) !important; /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f2f2f2), color-stop(14%,#ffffff), color-stop(100%,#ffffff)) !important; /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  #f2f2f2 0%,#ffffff 14%,#ffffff 100%) !important; /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  #f2f2f2 0%,#ffffff 14%,#ffffff 100%) !important; /* Opera 11.10+ */
background: -ms-linear-gradient(top,  #f2f2f2 0%,#ffffff 14%,#ffffff 100%) !important; /* IE10+ */
background: linear-gradient(to bottom,  #f2f2f2 0%,#ffffff 14%,#ffffff 100%) !important; /* W3C */
color:#787878 !important; font:normal 18px/22px 'Roboto',sans-serif; padding: 10px 9px !important; height:auto; border:1px solid #d2d2d2;
}
/* donation form step 1 - rightpart*/
body.skyblue .recent-donations h2{ border-bottom:1px solid #d9eff6;}
body.skyblue .recent-donations .bg {background:#e8f9ff; }
body.skyblue .recent-donations .donation-comment{border:1px solid #cfe3e9;}
body.skyblue .recent-donations .donation-comment p.amount{ color:#015e7d; }
body.skyblue .donation-rightpart .rightpart-box .bg .view-profile, body.skyblue .donation-rightpart .rightpart-box .bg .view-profile:hover, body.skyblue .donation-rightpart .rightpart-box .bg p span{color:#015e7d;}


/*Profile Title*/
body.skyblue .donation-detail .page-title { background:#1679C4 url(../../img/skyblue/pagebg.gif) repeat-x 0 -145px;}

/*progress & donation/pledge detail*/
body.skyblue .progress {box-shadow:0 1px 2px rgba(0, 0, 0, 0.1) inset!important; background-image:linear-gradient(to bottom, #E6E6E6, #ECECEC)!important;}
body.skyblue .progress .bar {background-color:#6abfff; background-size: 70px 70px;}
body.skyblue .raised-amount, body.skyblue .goal-amount {background:url(../../img/white-transperent-bg.png) repeat 0 0; color:#014c65; }

body.skyblue .btnDonatePledge {color:#fff; border-bottom:2px solid #015470; background: #30a1c6; /* Old browsers */
/* IE9 SVG, needs conditional override of 'filter' to 'none' */
background: url(data:image/svg+xml;base64,);
background: -moz-linear-gradient(top,  #30a1c6 0%, #04759a 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#30a1c6), color-stop(100%,#04759a)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  #30a1c6 0%,#04759a 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  #30a1c6 0%,#04759a 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top,  #30a1c6 0%,#04759a 100%); /* IE10+ */
background: linear-gradient(to bottom,  #30a1c6 0%,#04759a 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#30a1c6', endColorstr='#04759a',GradientType=0 ); /* IE6-8 */}  

body.skyblue .btnDonatePledge:hover, body.skyblue .btnDonatePledge:focus{color:#fff; background: #30a1c6; /* Old browsers */
/* IE9 SVG, needs conditional override of 'filter' to 'none' */
background: url(data:image/svg+xml;base64,);
background: -moz-linear-gradient(top,  #04759a 0%, #30a1c6 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#04759a), color-stop(100%,#30a1c6)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  #04759a 0%,#30a1c6 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  #04759a 0%,#30a1c6 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top,  #04759a 0%,#30a1c6 100%); /* IE10+ */
background: linear-gradient(to bottom,  #04759a 0%,#30a1c6 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#04759a', endColorstr='#30a1c6',GradientType=0 ); /* IE6-8 */}

body.skyblue .time {background:#eef3f5 url(../../img/icon-clock.png) no-repeat 9px center; border:3px solid #e6eef1; color:#2a2a2a;}
body.skyblue .time .day-remaining {color:#2a2a2a;}
body.skyblue .time .text {color:#2a2a2a; padding-left: 8px;}

body.skyblue .donation-needed .donation-slider { background:url(../../img/skyblue/slider.png) repeat-x 0px 8px;}
body.skyblue .donation-needed .donation-slider .slider {background:url(../../img/skyblue/slider.png) repeat-x 0px -38px; }
body.skyblue .donation-needed .donation-slider .slider span {border:1px solid #585858; color:#fff; background: #028cb9; /* Old browsers */
/* IE9 SVG, needs conditional override of 'filter' to 'none' */
background: url(data:image/svg+xml;base64,);
background: -moz-linear-gradient(top,  #028cb9 0%, #026484 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#028cb9), color-stop(100%,#026484)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  #028cb9 0%,#026484 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  #028cb9 0%,#026484 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top,  #028cb9 0%,#026484 100%); /* IE10+ */
background: linear-gradient(to bottom,  #028cb9 0%,#026484 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#028cb9', endColorstr='#026484',GradientType=0 ); /* IE6-8 */}

body.skyblue .donation-needed .total {background:#f5f5f5; color:#028cb9;}

body.skyblue .social-icons {margin:18px 0 10px;}
body.skyblue .social-icons img {margin:0 5px 10px 0;}

body.skyblue .donation-pledge-detail .article-block .main-article {border:1px solid #b2dbe8;}
body.skyblue .donation-pledge-detail .article-block .main-article .bg {background:#f8f8f8; }
body.skyblue .donation-pledge-detail .article-block .main-article .bg p span {color:#015e7d;}
body.skyblue .donation-pledge-detail .article-block .main-article input.fb-link { border:1px solid #e2e2e2; background:#fff; color:#4b7276;}
body.skyblue .donation-pledge-detail .article-block .main-article input.fb-link:focus{border-color: #999999;}
body.skyblue .donation-pledge-detail .article-block .main-article .btnShare {background:url(../../img/btn-share.gif) no-repeat 0 0;}
body.skyblue .view-profile {color:#015e7d; background: url(../../img/skyblue/arrow-right.gif) no-repeat right 8px; }
body.skyblue .view-profile:hover {color:#015e7d; }
body.skyblue .donation-pledge-detail h1 {color:#015e7d;}
body.skyblue .donation-pledge-detail .media .posted-by{ color:#8d8c8c;}
body.skyblue .donation-pledge-detail .media .posted-by span{color:#015e7d;}
body.skyblue .donation-pledge-detail .media .media-comment{ border:1px solid #e8e8e8;}
body.skyblue .donation-pledge-detail .media .media-heading{color:#015e7d; background:url(../../img/comment-bubble-top.gif) no-repeat 0px 0px; }
#payment_donation span.extra_amount {display:block;margin-left:0px;font-size:15px;}


