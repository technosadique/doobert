		                    required: true
						},
						oldertc: {
							required: true
						}
					},
			
					messages: {
						firstname: {
							required: 'Please enter first name.'
						},
						lastname: {
							required: 'Please enter last name.',							
						},
						email_add: {
							required: 'Please enter email.',
							email: 'Please enter valid email.'
						},
						user_password: {
							required: 'Please enter password.',							
						},
						tnc: {
							required: "Please accept terms of service and privacy policy."
						},
						oldertc: {
							required: "Please accept age terms and conditions."
						}
					},
			
					invalidHandler: function (event, validator) { //display error alert on form submit   

					},
	
					highlight: function (element) { // hightlight error inputs
						jQuery(element)
							.closest('.form-group').addClass('has-error'); // set error class to the control group
					},
	
					success: function (label) {
						label.closest('.form-group').removeClass('has-error');
						label.remove();
					},
	
					errorPlacement: function (error, element) {
						if (element.attr("name") == "tnc") { // insert checkbox errors after the container                  
							error.insertAfter(jQuery('#register_tnc_error'));
						}else if (element.attr("name") == "oldertc") { // insert checkbox errors after the container                  
							error.insertAfter(jQuery('#register_oldertc_error'));
						} else if (element.closest('.input-icon').size() === 1) {
							error.insertAfter(element.closest('.input-icon'));
						} else {
							//error.insertAfter(element);
							error.insertAfter(element.closest('.form-group'));
						}
					}
				
				});
				
        });
		
jQuery(".adopters-signup").live("click", function() {
  //alert("Sign Up As Driver!"); 
  jQuery('#captch_error').html('');
  var captcha_response = grecaptcha.getResponse();
  if(jQuery('#adopters_signup').valid() == false || captcha_response.length == 0) {
	//alert('form submit false');
	if(captcha_response.length == 0)
	{
		// Captcha is not Passed
		//alert('reCAPTCHA is unchecked, Check to proceed');
		jQuery('<span class=help-block" id="captch_error" style="color:#ff0000 !important;">reCAPTCHA is unchecked, Check to proceed.</span>" ').insertAfter(".g-recaptcha");
	}else{ jQuery('#captch_error').html(''); }
	return false;
	} else {
	//alert('form submit true');
	//call ajax
		var f_name  = jQuery('#firstname').val();
    	var l_name  = jQuery('#lastname').val();
		var email_add  = jQuery('#email_add').val();
		var user_password  = jQuery('#user_password').val();
		var personorg  = jQuery('#personorg').val();
		var page_ref  = jQuery('#page_ref').val();
		//alert('email'+email_add);
		//alert('password'+user_password);
		jQuery.ajax({
				type: "post",
				url: "http://www.doobert.com/appsignup.php",
				beforeSend: function(){
					 jQuery('.adopters-signup').hide();
					jQuery('#loader').show();
					},
				data: {	first_name:	f_name, 
						last_name: l_name,
						email_add: email_add,
						user_password: user_password,
						personorg: personorg,
						page_ref: page_ref,
						sign_up_as:'AD',
						},
				dataType:'json',
				complete: function(){
					jQuery('#loader').hide();
					jQuery('.adopters-signup').show();
					//alert('ajax call complete');
				  },
				  success:function(data){
				  	//alert(data);
					var t = JSON.parse(data);
					console.log(t);
					//alert(t);
					console.log(t['result']);
					console.log(t['err_msg']);
					//return false;
					if(t['result'] == 2)
					{
						//alert('error occured');
						//alert(data[0].err_msg);
						jQuery('.alert-danger').show();
						jQuery("#error_msg").html(t['err_msg']);
						return false;
					}else{
						
						setCookie('adopters_signup','adopters_signup',10);
						setCookie('userid',t['userid'],10);
						setCookie('user_name',t['user_name'],10);
						setCookie('last_name',t['last_name'],10);
						setCookie('email',t['email'],10);
						//window.location.href = "http://www.doobert.com/animal-rescue-volunteer/animal-rescue-drivers/";
						//window.location.href = "https://app.doobert.com/";
						window.location.href = "http://www.doobert.com/sign-up-thank-you/";
						return false;					
					}
				} 
		 });
						 				
	}
});
</script>
