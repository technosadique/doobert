					$this->db->join("tbl_transportation_leg_detail dp",'dp.in_transportation_leg_id=l.in_transportation_leg_id AND d.in_deleted = "0" AND dp.st_leg_type = "D"','LEFT');					
					$this->db->join("tbl_zip_code z","d.st_zip = z.zip_code","INNER");
