		//$this->db->order_by("tr.in_transportation_id","DESC");
		
		switch($sort)
		{
			case 'trid':
					$this->db->order_by("tr.in_transportation_id",$order);
			break;
			case 'transname':
					$this->db->order_by("tr.st_trans_name",$order);
