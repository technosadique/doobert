	   if(navigator.userAgent.match(/Android/i)){  
		   $('.android').css("display", "block");
		   $('.desktop-device').remove();
	   }
	   else
	   {
		   $('.android').remove();
		   $('.desktop-device').css("display", "block");
	   }
	   
	   
	   $('#type-org2').on('change', function () { 
          var url = $(this).val(); // get selected value
          if (url) { // require a URL
