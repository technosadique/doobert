.fbpage-wrap {width:100%;margin:0;padding:0;}
.fbpage {margin:0 auto;padding:0;max-width:1135px; text-align: center;}


/*header start*/
.header {margin:0;padding:0;width:100%;background:#fff;height:39px;padding:8px 0}
.header .logo {float:left;width:130px;padding-left:8px;}
.header .invite-btn-wrap {margin:0 auto;width:390px;}
.header .invite-btn-wrap a {background:#8dbf16;padding:5px 14px;font: 16px/35px 'OpenSansSemibold',Helvetica,Arial,sans-serif;border-bottom:3px solid #60a003;-webkit-border-radius:3px;-moz-border-radius:3px;border-radius:3px;color:#fff; margin-right: 5px;}
.header .invite-btn-wrap a:hover {background:#60a003;}
.header .fblink-wrap {float:right;width:auto;padding-top:8px;padding-right:15px;}
.header .invite-btn-wrap a .fa-home {font-size: 25px; vertical-align: middle;}
.home-icon {padding: 5px 7px !important;} 

.fbpage .top-menu-wrap {background:url(../images/bg_trans.png) repeat;padding:11px; display: inline-block; margin:0 auto;-webkit-box-shadow:0px 2px 10px 0px rgba(0, 78, 122, 0.29);-moz-box-shadow:0px 2px 10px 0px rgba(0, 78, 122, 0.29);box-shadow:0px 2px 10px 0px rgba(0, 78, 122, 0.29);border-bottom-left-radius:3px;border-bottom-right-radius:3px;}
.fbpage .top-menu-wrap ul li {list-style:none;display:inline-block;margin-right:8px;}
.fbpage .top-menu-wrap ul li a {display:inline-block; background:#df1051;padding:10px;-webkit-border-radius:3px;-moz-border-radius:3px;border-radius:3px;font-weight:600; font-size: 16px;}
.fbpage .top-menu-wrap ul li a.appbf-but {background-color: #4763b3;}
.fbpage .top-menu-wrap ul li a.appbf-but:hover {background-color: #304b95;}
.fbpage .top-menu-wrap ul li a.last {margin:0;}
.fbpage .top-menu-wrap ul li a:hover {background:#c50441;}
.fbpage .rescue-text {text-align:center;}
.fbpage .rescue-text h2 {font:700 25px/32px 'OpenSansBold',Helvetica,Arial,sans-serif;margin:32px 0 5px;padding:0}
.fbpage .rescue-text p {font:400 16px/20px 'OpenSansRegular',Helvetica,Arial,sans-serif;}
.fbpage .center-section {width:100%;margin:25px 0 34px;padding:0px;}/*border-bottom:1px solid #95d0e5; margin:25px 0 40px;padding:0 0 16px; rrmoved by 1Feb2016*/
.fbpage .box {float:left;width:273px;height:96px;background:#fff;-webkit-border-radius:3px;-moz-border-radius:3px;border-radius:3px;color:#004e7a;text-align:center;padding:18px 0;border-bottom-right-radius:0;-webkit-box-shadow:0px 2px 10px 0px rgba(0, 78, 122, 0.29);-moz-box-shadow:0px 2px 10px 0px rgba(0, 78, 122, 0.29);box-shadow:0px 2px 10px 0px rgba(0, 78, 122, 0.29);}
.fbpage .box p.title {font:700 23px/25px 'OpenSansBold',Helvetica,Arial,sans-serif;padding-bottom:10px;}
.fbpage .box .view-animal {background:#fe4d00;display:inline-block;color:#fff;padding:0 20px;font:700 16px/36px 'OpenSansBold',Helvetica,Arial,sans-serif;-webkit-border-radius:3px;-moz-border-radius:3px;border-radius:3px;}
.fbpage .box .right-corner {background:url(../images/right_corner.png) no-repeat right bottom;float:right;height:57px;margin-right:-24px;width:25px;}
.fbpage .center-section .donate { display:inline-block; width:auto; padding:0 7px 0 30px;}/*float:left;width:524px;padding:0 7px 0 30px;*/
.fbpage .center-section .centerbg {float:left;width:13px;margin-bottom:-44px;}
.fbpage .center-section .centerbg img {margin-top:11px;}
.fbpage .center-section .rescue { display:inline-block; width:auto; padding-right:30px;}/*float:left;width:523px;padding-right:30px;*/
.fbpage .center-section .rescue .box {float:right;border-bottom-left-radius:0;border-bottom-right-radius:3px;}
.fbpage .box .left-corner {background:url(../images/left_corner.png) no-repeat left bottom;float:left;height:59px;margin-left:-25px;width:25px;}
.fbpage .rescue-pic {padding-left:0px;}/*padding-left:10px;*/
.fbpage .center-menu-wrap {border-bottom:1px solid #95d0e5;padding-bottom:0px;text-align:center;}
.fbpage .center-menu-wrap ul {margin:0 0 18px;}
.fbpage .center-menu-wrap ul li {list-style:none;display:inline-block;}
.fbpage .center-menu-wrap ul li a { font:400 16px 'OpenSansRegular',Helvetica,Arial,sans-serif; background:#046c9a;padding:0 20px;line-height:42px;display:block;-webkit-border-radius:3px;-moz-border-radius:3px;border-radius:3px;margin-right:20px;}
.fbpage .center-menu-wrap ul li a:hover{background:#02628c}
.fbpage .center-menu-wrap .centerbg {width:13px;margin:0 auto;}
.fbpage .center-menu-wrap .centerbg img {margin-bottom:-28px;}
.fbpage .professional-wrap{ margin-bottom:41px;}
.fbpage .professional-wrap .down-section {margin:0 auto;padding:45px 0 0 0;display:inline-block;}/*margin:45px auto 54px;padding:0;width:920px;height:96px;*/
.fbpage .professional-wrap .down-section div.bgnone {background:none;padding:0;margin-top:-20px;width:186px;text-align:center;}
.fbpage .professional-wrap .down-section div {float:left;background:#fff;padding:28px 15px;position:relative;-webkit-border-radius:3px;-moz-border-radius:3px;border-radius:3px;}
.fbpage .professional-wrap .down-section div a {font:16px 'OpenSansRegular',Helvetica,Arial,sans-serif; background:#fe4d00;padding:6px 25px;-webkit-border-radius:3px;-moz-border-radius:3px;border-radius:3px;font-weight:600;}
.fbpage .professional-wrap .down-section div a:hover {background:#ed4800;}
.fbpage .professional-wrap .down-section div span.arrows {display:inline-block;background:url(../images/bottom-arrow.png) no-repeat left top;width:20px;height:10px;position:absolute}/*background:url(../images/arrows.gif) no-repeat left top;width:17px;height:36px;*/
.fbpage .professional-wrap .down-section div .bottom {left:163px;top:78px}
.fbpage .professional-wrap .down-section div .right {right:-16px;top:21px}
.fbpage .professional-wrap .down-section div .left {left:-16px;top:21px;background:url(../images/arrows.gif) no-repeat right top !important;}
.fbpage .professional-wrap .rescue-text h2{ margin-top:20px;}
.footer {background:#262626;text-align:center;font:400 13px/18px 'OpenSansRegular',Helvetica,Arial,sans-serif;padding:10px 0;color:#fff;clear:both;}
.footer a {text-decoration:underline !important;color:#fff;}
.footer a:hover {text-decoration:none !important;}
span.separter {background:url(../images/seprater.gif) no-repeat top right;padding-right:5px;margin-right:8px; color: #fff;}

