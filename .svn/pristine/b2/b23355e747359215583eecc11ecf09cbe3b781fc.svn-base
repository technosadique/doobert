.org {background:#ee6d35;}
.org:hover {background:#d6541c;}
.as-nav {display:none;}

.inline_small .popup-header h2 {color:#189487;display:inline-block;margin:0;padding:0 10px 0 0;font:600 18px/25px 'OpenSansSemibold',Helvetica,Arial,sans-serif;}
.inline_small  .popup-content {padding:20px 0;}
