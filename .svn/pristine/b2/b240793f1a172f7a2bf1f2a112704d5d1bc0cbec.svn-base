#: classes/class-settings.php:205
msgid "Email subject: "
msgstr "Email Betreff:"

# @ stc_textdomain
#: classes/class-settings.php:215
msgid "Resend post on update"
msgstr "Beitrag bei Änderung erneut senden"

# @ stc_textdomain
#: classes/class-settings.php:222
msgid "Resend:"
msgstr "Erneut senden:"

# @ stc_textdomain
#: classes/class-settings.php:232
msgid "Stylesheet (CSS) settings"
msgstr "Stylesheet (CSS) Einstellungen"

