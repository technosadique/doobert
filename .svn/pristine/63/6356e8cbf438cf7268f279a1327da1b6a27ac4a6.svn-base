#custom-sidebar {width: 440px!important; margin-top: -38px}
.single #custom-sidebar {margin-top: 0;}
.sidebar {width: 440px!important;}
/*.col-md-4.col-sm-4.col-xs-4.rightbar {padding-top: 0;}*/
#custom-sidebar .widget_stc_widget, #right-sidebar .widget_stc_widget  {border:1px solid #d9d9d9; padding: 18px 20px; box-sizing: border-box; margin-bottom: 40px; background: none !important; border-radius:0!important;}
#right-sidebar .widget_stc_widget {margin-bottom: 18px;}
#custom-sidebar .widget_stc_widget h3, #right-sidebar .widget_stc_widget h3 {font:600 18px/21px 'Roboto', sans-serif !important; color: #6c6d72!important; padding-bottom:21px; margin-top: 0px;}
#custom-sidebar .widget_stc_widget h3:before, #right-sidebar .widget_stc_widget h3:before{font:22px 'Material Icons'!important; content: "\E02F"; vertical-align: middle; text-decoration: none; display: inline-block; color:#f35613!important; padding-right:12px;}
#custom-sidebar .widget_stc_widget h3:after, #right-sidebar .widget_stc_widget h3:after{content: none}
#custom-sidebar .widget_stc_widget label, #right-sidebar .widget_stc_widget label{display:none;}
#custom-sidebar .widget_stc_widget .select-style, #right-sidebar .widget_stc_widget .select-style{margin-bottom:15px;}
#custom-sidebar .widget_stc_widget .form-group, #right-sidebar .widget_stc_widget .form-group{position:relative; margin-bottom: 0px;}
#stc-subscribe-btn {float: left; font: 600 14px "Open Sans", Arial, Helvetica, sans-serif; color: #fff; padding: 10px 18px 11px; -webkit-border-radius: 0 4px 4px 0 !important; -moz-border-radius: 0 4px 4px 0 !important; -ms-border-radius: 0 4px 4px 0 !important; -o-border-radius: 0 4px 4px 0 !important; border-radius: 0 4px 4px 0 !important;   background: #ee6d35; border: none; position: absolute; right: 0px; height: auto; text-transform: uppercase; margin-top: 0px; box-shadow: none !important;}
.widget_stc_widget input[type="text"] {width: 95%; font:400 15px/21px 'Roboto', Arial, Helvetica, sans-serif !important; color: #6c6d72; border: 1px solid #f0f0f0!important; height: 41px;}

#content .related-posts ul li a, #content .related-posts ul li h4 {display: inline-block; vertical-align: top; margin-top: 0;}
#content .related-posts ul li h4 {width: 67%;}

.rpwe-block li {margin-bottom: 0!important}
.rpwe_widget .rpwe-block h3 a {line-height: 18px;}

#rpwe_widget-3 h3, #rpwe_widget-2 h3 {margin-top: 0; font:500 20px/21px 'Roboto', sans-serif !important; color: #6c6d72; padding-bottom: 21px; background: none; padding-left: 0;}
#rpwe_widget-3 h3:after, #rpwe_widget-2 h3:after {border-bottom:5px solid #e5e5e5; width:20%; content: ''; display: table; padding-top: 6px;}

.rpwe_widget .rpwe-block li h3 {padding-bottom: 0!important;}
.rpwe_widget .rpwe-block li h3:after {border-bottom:none!important; width:auto; content: ''; display: table; padding-top: 0;}
.rpwe_widget .rpwe-block h3 a {font:500 18px/24px 'Roboto', sans-serif !important; color: #f35613;}
.rpwe_widget .rpwe-block .rpwe-time {font-size: 16px!important; color: #bbb!important}
.rpwe_widget .rpwe-block .rpwe-summary {padding-top: 7px!important;}

